{
  "CreditMemo": {
    "name": "CreditMemo",
    "description": "Represents a document that’s used to reduce the amount that a buyer owes a seller under the terms of an earlier invoice.",
    "properties": {
      "Balance": {
        "type": "currency",
        "description": "The amount of the credit memo that's available for allocation.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "BillToContactId": {
        "type": "reference",
        "description": "This field is inherited from the account’s Bill to Account field.",
        "format": "salesforce-id",
        "x-object": "Contact",
        "maxLength": 18,
        "nullable": true
      },
      "BillingAccountId": {
        "type": "reference",
        "description": "Required. The ID of the customer account associated with this credit memo.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": false
      },
      "Category": {
        "type": "picklist",
        "description": "Specifies whether the credit memo is created due to writing off an invoice, converting negative invoice lines, or referencing an existing record."
      },
      "CorpCrcyCnvTotAmtWithTax": {
        "type": "double",
        "description": "The sum of the total amount with tax on the credit memo in corporate currency. Available in API version 63.0 and later."
      },
      "CorporateCurrencyCvsnDate": {
        "type": "date",
        "description": "The date on which the credit memo amounts are converted to the corporate currency. Available in API version 63.0 and later."
      },
      "CorporateCurrencyCvsnRate": {
        "type": "double",
        "description": "The exchange rate that's used to convert credit memo amounts to the corporate currency. Available in API version 63.0 and later."
      },
      "CorporateCurrencyIsoCode": {
        "type": "string",
        "description": "The currency ISO code of the corporate currency. Available in API version 63.0 and later."
      },
      "CreationMode": {
        "type": "picklist",
        "description": "Specifies whether the credit memo originated in Salesforce or an external system."
      },
      "CreditDate": {
        "type": "date",
        "description": "The date when the credit memo was posted.",
        "format": "date",
        "nullable": true,
        "readOnly": true
      },
      "CreditMemoNumber": {
        "type": "string",
        "description": "The number of the credit memo.",
        "maxLength": 10,
        "nullable": true
      },
      "Description": {
        "type": "string",
        "description": "Additional details about the credit memo.",
        "maxLength": 255,
        "nullable": true
      },
      "DocumentNumber": {
        "type": "string",
        "description": "Required. The auto-generated reference number for the credit memo.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "EffectiveDate": {
        "type": "date",
        "description": "The effective date of the credit memo. If this field is empty, the credit date is used. For reporting purposes only; this field drives no other logic."
      },
      "ExternalReference": {
        "type": "string",
        "description": "An external system’s ID for the credit memo."
      },
      "ExternalReferenceDataSource": {
        "type": "string",
        "description": "The name of the external system that also contains the credit memo."
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The timestamp when the current user last accessed a credit memo record indirectly, for example, through a list view or related record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The timestamp when the current user last viewed a credit memo record. If this value is null, it’s possible that the user only accessed the credit memo record or a related list view (LastReferencedDate), but not viewed the credit memo record itself.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LegalEntityAccountingPeriodId": {
        "type": "reference",
        "description": "The legal entity accounting period record that’s related to the credit memo. This field is available in API version 65.0 and later."
      },
      "LegalEntityId": {
        "type": "reference",
        "description": "The legal entity related to the credit memo. This field is available in API version 65.0 and later."
      },
      "NetCreditsApplied": {
        "type": "currency",
        "description": "The total difference between the credit applied to and credit unapplied from the invoice."
      },
      "OwnerId": {
        "type": "reference",
        "description": "Required. The ID of the user who owns a credit memo record.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ReasonCode": {
        "type": "picklist",
        "description": "The reason code for the credit memo's category. For example, BD can be the reason code when the credit memo's category is InvoiceWriteOff and the reason for the invoice write-off is bad debt. This field is available in API version 64.0 and later."
      },
      "ReferenceEntityId": {
        "type": "reference",
        "description": "The ID of the record that this credit memo was generated from. For example, the order, order summary, or invoice.",
        "format": "salesforce-id",
        "x-object": "OrderSummary",
        "maxLength": 18,
        "nullable": true
      },
      "SettlementLevel": {
        "type": "picklist",
        "description": "Specifies whether the credit memo amount was applied to an invoice or an invoice line."
      },
      "SourceAction": {
        "type": "picklist",
        "description": "Specifies which Salesforce API created the credit memo."
      },
      "Status": {
        "type": "picklist",
        "description": "Required. The status of the credit memo.",
        "format": "enum",
        "enum": [
          "Draft",
          "Pending",
          "Posted",
          "Canceled",
          "Error",
          "Voided"
        ],
        "maxLength": 255,
        "nullable": false
      },
      "TotalAdjustmentAmount": {
        "type": "currency",
        "description": "The sum of the TotalAmount field values for the credit memo’s adjustment lines.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAdjustmentAmountWithTax": {
        "type": "currency",
        "description": "The sum of the credit memo’s adjustment line amounts, including tax. Available in API versions 62.0 and 63.0.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAdjustmentTaxAmount": {
        "type": "currency",
        "description": "The sum of the credit memo’s adjustment line tax. Adjustment line balances are excluded. Available in API versions 62.0 and 63.0.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAmount": {
        "type": "currency",
        "description": "The sum of the credit memo’s TotalLineAmount and TotalAdjustmentAmount field values.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAmountWithTax": {
        "type": "currency",
        "description": "The total credit memo amount, with tax included.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalChargeAmount": {
        "type": "currency",
        "description": "The sum of the TotalAmount field values for the credit memo’s charge lines.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalChargeAmountWithTax": {
        "type": "currency",
        "description": "The sum of the credit memo’s charge line amounts, including tax. Available in API versions 62.0 and 63.0.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalChargeTaxAmount": {
        "type": "currency",
        "description": "The sum of the credit memo’s charge tax amount. Available in API versions 62.0 and 63.0.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalTaxAmount": {
        "type": "currency",
        "description": "The sum of the TotalAmount field values for the credit memo’s tax lines.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "Id": {
        "type": "string",
        "description": "Credit Memo ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "nullable": false,
        "readOnly": true
      },
      "TaxLocaleType": {
        "type": "string",
        "format": "enum",
        "enum": [
          "Net",
          "Gross",
          "Automatic"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "AppType": {
        "type": "picklist",
        "description": "Read-only field that indicates which Salesforce application generated the credit memo."
      },
      "CurrencyIsoCode": {
        "type": "picklist",
        "description": "Three-letter ISO 4217 currency code associated with the credit memo."
      },
      "TotalCreditAmountApplied": {
        "type": "currency",
        "description": "Credit memo amount that's been applied to invoices."
      },
      "TotalCreditAmountUnapplied": {
        "type": "currency",
        "description": "Credit memo amount that's been unapplied from invoices."
      }
    },
