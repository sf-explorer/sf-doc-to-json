{
  "EmailTemplate": {
    "name": "EmailTemplate",
    "description": "Represents a template for an email, mass email, list email, or Sales Engagement email. Supported in first-generation managed packages only.",
    "properties": {
      "Id": {
        "type": "string",
        "description": "Email Template ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "Name": {
        "type": "string",
        "description": "The email template name.",
        "maxLength": 80,
        "nullable": false
      },
      "DeveloperName": {
        "type": "string",
        "description": "The unique name of the object in the API. This name can contain only underscores and alphanumeric characters, and must be unique in your org. It must begin with a letter, not include spaces, not end with an underscore, and not contain two consecutive underscores. In managed packages, this field prevents naming conflicts on package installations. With this field, a developer can change the object’s name in a managed package and the changes are reflected in a subscriber’s organization. Label is Template Unique Name. Note When creating large sets of data, always specify a unique DeveloperName for each record. If no DeveloperName is specified, performance may slow while Salesforce generates one for each record.",
        "maxLength": 80,
        "nullable": false
      },
      "NamespacePrefix": {
        "type": "string",
        "description": "A unique string to distinguish this template from any others. For example, if this template is being using by a flow, use the NamespacePrefix to uniquely identify the templates in multiple flow instances.",
        "maxLength": 15,
        "nullable": true,
        "readOnly": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "ID of the owner of the template.",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false
      },
      "FolderId": {
        "type": "reference",
        "description": "ID of the folder that contains the template.",
        "format": "salesforce-id",
        "x-objects": [
          "Folder",
          "Organization",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "FolderName": {
        "type": "string",
        "description": "The name of the folder that contains the template.",
        "maxLength": 256,
        "nullable": true,
        "readOnly": true
      },
      "BrandTemplateId": {
        "type": "reference",
        "description": "Required. ID of the BrandTemplate associated with this email template. The brand template supplies letterhead information for the email template.",
        "format": "salesforce-id",
        "x-object": "BrandTemplate",
        "maxLength": 18,
        "nullable": true
      },
      "EnhancedLetterheadId": {
        "type": "reference",
        "description": "ID of the enhanced letterhead associated with the email template. Note To use an enhanced letterhead, associate it with a Lightning email template that uses the HML merge language.",
        "format": "salesforce-id",
        "x-object": "EnhancedLetterhead",
        "maxLength": 18,
        "nullable": true
      },
      "TemplateStyle": {
        "type": "picklist",
        "description": "Style of the template.",
        "format": "enum",
        "enum": [
          "none",
          "freeForm",
          "formalLetter",
          "promotionRight",
          "promotionLeft",
          "newsletter",
          "products"
        ],
        "maxLength": 40,
        "nullable": false,
        "readOnly": true
      },
      "IsActive": {
        "type": "boolean",
        "description": "Indicates that this template is active if true, or inactive if false.",
        "nullable": false
      },
      "TemplateType": {
        "type": "picklist",
        "description": "Type of template.",
        "format": "enum",
        "enum": [
          "text",
          "html",
          "custom",
          "visualforce"
        ],
        "maxLength": 40,
        "nullable": false,
        "readOnly": true
      },
      "Encoding": {
        "type": "picklist",
        "description": "Character set encoding for the template.",
        "format": "enum",
        "enum": [
          "UTF-8",
          "ISO-8859-1",
          "Shift_JIS",
          "ISO-2022-JP",
          "EUC-JP",
          "ks_c_5601-1987",
          "Big5",
          "GB2312",
          "Big5-HKSCS",
          "x-SJIS_0213"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "Description": {
        "type": "string",
        "description": "The email template description. This can be useful to describe the reason for creating the template or its intended use.",
        "maxLength": 4000,
        "nullable": true
      },
      "Subject": {
        "type": "string",
        "description": "The email subject.",
        "maxLength": 1000,
        "nullable": true
      },
      "HtmlValue": {
        "type": "textarea",
        "description": "This field contains the content of the email message, including HTML coding to render the email message. Limit: 384 KB.",
        "maxLength": 384000,
        "nullable": true
      },
      "Body": {
        "type": "textarea",
        "description": "Content of the email. Limit: 384 KB.",
        "maxLength": 384000,
        "nullable": true
      },
      "TimesUsed": {
        "type": "int",
        "description": "Number of times this email template has been used.",
        "nullable": true,
        "readOnly": true
      },
      "LastUsedDate": {
        "type": "dateTime",
        "description": "Date and time when this email template was last used.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "description": "Last Modified By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "description": "System Modstamp",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "ApiVersion": {
        "type": "double",
        "description": "The API version if this is a Visualforce email template. Every Visualforce email template has an API version specified at creation.",
        "nullable": true,
        "multipleOf": 0.1
      },
      "Markup": {
        "type": "textarea",
        "description": "The Visualforce markup, HTML, JavaScript, or any other Web-enabled code that defines the content of the template.",
        "maxLength": 1048576,
        "nullable": true
      },
      "UiType": {
        "type": "string",
        "description": "UI Type",
        "format": "enum",
        "enum": [
          "Aloha",
          "SFX",
          "SFX_Sample"
        ],
        "maxLength": 40,
        "nullable": true
      },
