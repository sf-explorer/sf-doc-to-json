{
  "Expense": {
    "name": "Expense",
    "description": "Represents an expense linked to a work order. Service resource technicians can log expenses, such as tools or travel costs.",
    "properties": {
      "AccountId": {
        "type": "reference",
        "description": "The ID of the account associated with the linked work order.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "Amount": {
        "type": "currency",
        "description": "The amount of the expense.",
        "format": "currency",
        "nullable": false,
        "multipleOf": 0.01
      },
      "CurrencyIsoCode": {
        "type": "picklist",
        "description": "Available only if the multicurrency feature is enabled. Contains the ISO code for any currency allowed by the organization.",
        "format": "enum",
        "enum": [
          "USD"
        ],
        "maxLength": 3,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "A description for the expense.",
        "maxLength": 32000,
        "nullable": true
      },
      "Discount": {
        "type": "percent",
        "description": "The percentage deducted from the Subtotal price. Available in version 51.0 and later."
      },
      "ExpenseEndDate": {
        "type": "date",
        "description": "If the expense was incurred over multiple days, the Expense End Date is the last day that the expense covers.",
        "format": "date",
        "nullable": true
      },
      "ExpenseNumber": {
        "type": "string",
        "description": "The number that uniquely identifies the expense.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ExpenseStartDate": {
        "type": "date",
        "description": "If the expense was incurred over multiple days, the Expense Start Date is the first day that the expense covers.",
        "format": "date",
        "nullable": true
      },
      "ExpenseType": {
        "type": "picklist",
        "description": "The type of expense. Possible values are: Billable Non-Billable The default value is Billable.",
        "format": "enum",
        "enum": [
          "Billable",
          "Non-Billable"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The timestamp for when the current user last viewed a record related to this record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The timestamp for when the current user last viewed this record. If this value is null, this record might only have been referenced (LastReferencedDate) and not viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "The ID of the user who owns the expense record.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "Quantity": {
        "type": "double",
        "description": "The number of items purchased in this record. Available in version 51.0 and later."
      },
      "Subtotal": {
        "type": "currency",
        "description": "The subtotal price calculated as the product of Quantity and UnitPrice. Available in version 51.0 and later."
      },
      "Title": {
        "type": "string",
        "description": "A title that identifies the expense.",
        "maxLength": 255,
        "nullable": true
      },
      "TotalPrice": {
        "type": "currency",
        "description": "The total price of the transaction which is equal to the discounted subtotal: Subtotal - (Discount * Subtotal). Available in version 51.0 and later."
      },
      "TransactionDate": {
        "type": "date",
        "description": "The day that the expense was incurred, or the payment date for the expense.",
        "format": "date",
        "nullable": false
      },
      "UnitPrice": {
        "type": "currency",
        "description": "The price of one item on the record. Available in version 51.0 and later."
      },
      "WorkOrderId": {
        "type": "reference",
        "description": "The ID of the work order associated with the expense.",
        "format": "salesforce-id",
        "x-object": "WorkOrder",
        "maxLength": 18,
        "nullable": true
      },
      "Id": {
        "type": "string",
