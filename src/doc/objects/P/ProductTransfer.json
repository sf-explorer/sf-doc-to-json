{
  "ProductTransfer": {
    "name": "ProductTransfer",
    "description": "Represents the transfer or the movement of a product item between two inventory locations.",
    "properties": {
      "Description": {
        "type": "textarea",
        "description": "The description of the product transfer.",
        "maxLength": 32000,
        "nullable": true
      },
      "DestinationLocationId": {
        "type": "reference",
        "description": "The place the product is to be delivered.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": true
      },
      "ExpectedPickupDate": {
        "type": "dateTime",
        "description": "The date when the product is expected to be picked up.",
        "format": "date-time",
        "nullable": true
      },
      "IsReceived": {
        "type": "boolean",
        "description": "Specifies whether the product was received (true) or not (false).",
        "nullable": false
      },
      "IsProduct2Serialized": {
        "type": "boolean",
        "description": "Specifies whether the product associated with the product transfer is a serialized product (true) or not (false).",
        "nullable": false,
        "readOnly": true
      },
      "IsSent": {
        "type": "boolean",
        "description": "Specifies whether the product was sent as a part of the transfer (true) or not (false)."
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The timestamp when the current user last accessed this record indirectly, for example, through a list view or related record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The timestamp when the current user last viewed this record or list view. If this value is null, and LastReferenceDate is not null, the user accessed this record or list view indirectly.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the product transfer.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "Product2Id": {
        "type": "reference",
        "description": "The product that’s associated with the product transfer.",
        "format": "salesforce-id",
        "x-object": "Product2",
        "maxLength": 18,
        "nullable": true
      },
      "Product2TransferRecordMode": {
        "type": "reference",
        "description": "If serialized, specifies when the serial number is recorded. It is visible on the product transfer as a read-only field depending on the field-level security. Possible values are: SendAndReceive —The serial number is recorded when sending or receiving. ReceiveOnly —The serial number is recorded when receiving only."
      },
      "ProductRequestId": {
        "type": "reference",
        "description": "The product request that’s associated with the product transfer",
        "format": "salesforce-id",
        "x-object": "ProductRequest",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ProductRequestLineItemId": {
        "type": "reference",
        "description": "The product request line item associated with the product transfer.",
        "format": "salesforce-id",
        "x-object": "ProductRequestLineItem",
        "maxLength": 18,
        "nullable": true
      },
      "ProductTransferNumber": {
        "type": "string",
        "description": "An auto-generated number that identifies the product transfer.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "QuantityReceived": {
        "type": "double",
        "description": "The quantity of product received at the destination location.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "QuantitySent": {
        "type": "double",
        "description": "The quantity of product sent from the source location.",
        "nullable": false,
        "multipleOf": 0.01
      },
