{
  "ServiceAppointment": {
    "name": "ServiceAppointment",
    "description": "Represents an appointment to complete work for a customer in Field Service, Lightning Scheduler,Intelligent Appointment Management, and Virtual Care.",
    "properties": {
      "ActualDuration": {
        "type": "double",
        "description": "The number of minutes that it took the resource to complete the appointment after arriving at the address. When values are first added to the Actual Start and Actual End fields, the Actual Duration is automatically populated to list the difference between the Actual Start and Actual End. If the Actual Start and Actual End fields are subsequently updated, the Actual Duration field doesn’t re-update, but you can manually update it.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "ActualEndTime": {
        "type": "dateTime",
        "description": "The actual date and time the appointment ended.",
        "format": "date-time",
        "nullable": true
      },
      "ActualStartTime": {
        "type": "dateTime",
        "description": "The actual date and time the appointment started.",
        "format": "date-time",
        "nullable": true
      },
      "AdditionalInformation": {
        "type": "string",
        "description": "Represents additional information about the service appointment.",
        "maxLength": 255,
        "nullable": true
      },
      "Address": {
        "type": "address",
        "description": "The address where the appointment is taking place. The address is inherited from the parent record if the parent record is a work order or work order line item.",
        "nullable": false,
        "readOnly": true
      },
      "AppointmentNumber": {
        "type": "string",
        "description": "An auto-assigned number that identifies the appointment.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ArrivalWindowEndTime": {
        "type": "dateTime",
        "description": "The end of the window of time in which the technician is scheduled to arrive at the site. This window is typically larger than the Scheduled Start and End window to allow time for delays and scheduling changes. You may choose to share the Arrival Window Start and End with the customer, but keep the Scheduled Start and End internal-only.",
        "format": "date-time",
        "nullable": true
      },
      "ArrivalWindowStartTime": {
        "type": "dateTime",
        "description": "The beginning of the window of time in which the technician is scheduled to arrive at the site. This window is typically larger than the Scheduled Start and End window to allow time for delays and scheduling changes. You may choose to share the Arrival Window Start and End with the customer, but keep the Scheduled Start and End internal-only.",
        "format": "date-time",
        "nullable": true
      },
      "CancellationReason": {
        "type": "string",
        "description": "The reason for the service appointment cancellation.",
        "maxLength": 255,
        "nullable": true
      },
      "City": {
        "type": "string",
        "description": "The city where the appointment is completed. Maximum length is 40 characters.",
        "maxLength": 40,
        "nullable": true
      },
      "Comments": {
        "type": "string",
        "description": "The comments for the service appointment.",
        "maxLength": 255,
        "nullable": true
      },
      "Country": {
        "type": "string",
        "description": "The country where the work order is completed. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the appointment.",
        "maxLength": 32000,
        "nullable": true
      },
      "DueDate": {
        "type": "dateTime",
        "description": "The date by which the appointment must be completed. Earliest Start Permitted and Due Date typically reflect terms in the customer’s service-level agreement.",
        "format": "date-time",
        "nullable": false
      },
      "Duration": {
        "type": "double",
        "description": "The estimated length of the appointment. If the parent record is work order or work order line item, the appointment inherits its parent’s duration, but it can be manually updated. The duration is in minutes or hours based on the value selected in the Duration Type field.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "DurationType": {
        "type": "picklist",
        "description": "The unit of the Duration: Minutes or Hours.",
        "format": "enum",
        "enum": [
          "Hours",
          "Minutes"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "EarliestStartTime": {
        "type": "dateTime",
        "description": "The date after which the appointment must be completed. Earliest Start Permitted and Due Date typically reflect terms in the customer’s service-level agreement.",
        "format": "date-time",
        "nullable": false
      },
      "GeocodeAccuracy": {
        "type": "picklist",
        "description": "The level of accuracy of a location’s geographical coordinates compared with its physical address. Usually provided by a geocoding service based on the address’s latitude and longitude coordinates. Note This field is available in the API only.",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the service appointment was last modified. Its label in the user interface is Last Modified Date.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the service appointment was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "Latitude": {
        "type": "double",
        "description": "Used with Longitude to specify the precise geolocation of the address where the service appointments is completed. Acceptable values are numbers between –90 and 90 with up to 15 decimal places. To integrate data from an external data source for latitude, map your data to the ServiceAppointment.Latitude and not the ServiceAppointment.FSL__InternalSLRGeolocation__Latitude__s field. Note This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Longitude": {
        "type": "double",
        "description": "Used with Latitude to specify the precise geolocation of the address where the service appointment is completed. Acceptable values are numbers between –180 and 180 with up to 15 decimal places. To integrate data from an external data source for longitude, map your data to the ServiceAppointment.Longitude and not the ServiceAppointment.FSL__InternalSLRGeolocation__Longitude__s field. Note This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the service appointment.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ParentRecordId": {
        "type": "reference",
        "description": "The parent record associated with the appointment. The parent record can’t be updated after the service appointment is created.",
        "format": "salesforce-id",
        "x-objects": [
          "Account",
          "Asset",
          "CareProgramEnrollee",
          "Case",
          "Lead",
          "Opportunity",
          "WorkOrder",
          "WorkOrderLineItem"
        ],
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ParentRecordType": {
        "type": "string",
        "description": "(Read only) The type of parent record: Account, Asset, Lead, Opportunity, Work Order, or Work Order Line Item.",
        "maxLength": 50,
        "nullable": true,
        "readOnly": true
      },
      "PostalCode": {
        "type": "string",
        "description": "The postal code where the work order is completed. Maximum length is 20 characters.",
        "maxLength": 20,
        "nullable": true
      },
      "SchedEndTime": {
        "type": "dateTime",
        "description": "The time at which the appointment is scheduled to end. If you are using the Field Service managed package with the scheduling optimizer, this field is populated once the appointment is assigned to a resource. Scheduled End – Scheduled Start = Estimated Duration.",
        "format": "date-time",
        "nullable": true
      },
      "SchedStartTime": {
        "type": "dateTime",
        "description": "The time at which the appointment is scheduled to start. If you are using the Field Service managed package with the scheduling optimizer, this field is populated once the appointment is assigned to a resource.",
        "format": "date-time",
        "nullable": true
      },
      "ServiceTerritoryId": {
        "type": "reference",
        "description": "The service territory associated with the appointment. If the parent record is a work order or work order line item, the appointment inherits its parent’s service territory.",
        "format": "salesforce-id",
        "x-object": "ServiceTerritory",
        "maxLength": 18,
        "nullable": true
      },
      "State": {
        "type": "string",
        "description": "The state where the service appointment is completed. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Status": {
        "type": "picklist",
        "description": "The status of the appointment. The picklist includes the following values, which can be customized: None—Default value. Scheduled—Appointment has been assigned to a service resource. Dispatched—Assigned service resource has been notified about their assignment. In Progress—Work has begun. Completed—Work is complete. Cannot Complete—Work could not be completed. Canceled—Work is canceled, typically before any work began While you can set the status to null via the API, setting the status to null returns an error. To prevent errors, use one of the picklist values.",
        "format": "enum",
        "enum": [
          "None",
          "Scheduled",
          "Dispatched",
          "Travel",
          "In Progress",
          "Cannot Complete",
          "Completed",
          "Canceled",
          "Approaching"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "StatusCategory": {
        "type": "picklist",
        "description": "The category that each Status value falls into. The Status Category field’s values are identical to the default Status values.If you create custom Status values, you must indicate which category it belongs to. For example, if you create a Customer Absent value, you may decide that it belongs in the Cannot Complete category. To learn which processes reference StatusCategory, see How are Status Categories Used?",
        "format": "enum",
        "enum": [
          "None",
          "Scheduled",
          "Dispatched",
          "InProgress",
          "CannotComplete",
          "Completed",
          "Canceled",
          "CheckedIn"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
      "Street": {
        "type": "textarea",
        "description": "The street number and name where the service appointment is completed.",
        "maxLength": 255,
        "nullable": true
