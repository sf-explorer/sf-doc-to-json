        "format": "enum",
        "enum": [
          "MultiChoice",
          "RadioButton",
          "FreeText",
          "Date",
          "Rating",
          "CSAT",
          "Slider",
          "Picklist",
          "NPS",
          "StackRank",
          "Currency",
