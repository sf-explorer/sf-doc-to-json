{
  "ServiceResource": {
    "name": "ServiceResource",
    "description": "Represents a service technician or service crew in Field Service and Salesforce Scheduler, or an agent in Workforce Engagement.",
    "properties": {
      "Id": {
        "type": "string",
        "description": "Resource ID",
