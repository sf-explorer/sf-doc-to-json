        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "AssetId": {
        "type": "string",
        "description": "Asset ID",
        "format": "salesforce-id",
        "x-object": "Asset",
        "maxLength": 18,
        "nullable": true
