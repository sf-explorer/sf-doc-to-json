{
  "ServiceTerritory": {
    "name": "ServiceTerritory",
    "description": "Represents a dealer location where service resources are located and service appointments are performed.",
    "properties": {
      "Address": {
        "type": "address",
        "description": "An address to associate with a dealer’s location.",
        "nullable": true,
        "readOnly": true
      },
      "City": {
        "type": "string",
        "description": "The city of the associated address. The Automotive appointment booking flows for test drives and vehicle services use the dealer name or city name as search input for the appointment location. Maximum length is 40 characters.",
        "maxLength": 40,
        "nullable": true
      },
      "Country": {
        "type": "string",
        "description": "The country to associate with the territory. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the territory.",
        "maxLength": 32000,
        "nullable": true
      },
      "GeocodeAccuracy": {
        "type": "picklist",
        "description": "The level of accuracy of a location’s geographical coordinates compared with its physical address. Usually provided by a geocoding service based on the address’s latitude and longitude coordinates.",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "IsActive": {
        "type": "boolean",
        "description": "Indicates whether the service territory is meant to be used (true) or not (false). If a territory is inactive, you can’t add members to it or link it to service appointments.",
        "nullable": false
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the record was last modified.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the record was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "Latitude": {
        "type": "double",
        "description": "Used with Longitude to specify the precise geolocation of the address where the service appointments is completed. Acceptable values are numbers between –90 and 90 with up to 15 decimal places.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Longitude": {
        "type": "double",
        "description": "Used with Latitude to specify the precise geolocation of the address where the service appointment is completed. Acceptable values are numbers between –180 and 180 with up to 15 decimal places.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Name": {
        "type": "string",
        "description": "The name of a dealer location. The Automotive appointment booking flows for test drives and vehicle services use the dealer name or city name as search input for the appointment location.",
        "maxLength": 255,
        "nullable": false
      },
      "OperatingHoursId": {
        "type": "reference",
        "description": "The territory’s operating hours, which indicate when service appointments within the territory can occur. Service resources who are members of a territory and work types related to a territory automatically inherit the territory’s operating hours unless different hours are specified on the resource or the work type record.",
        "format": "salesforce-id",
        "x-object": "OperatingHours",
        "maxLength": 18,
        "nullable": false
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the record.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ParentTerritoryId": {
        "type": "reference",
        "description": "The territory’s parent service territory, if it has one. A service territory hierarchy can contain up to 10,000 territories.",
        "format": "salesforce-id",
        "x-object": "ServiceTerritory",
        "maxLength": 18,
        "nullable": true
      },
      "PostalCode": {
        "type": "string",
        "description": "The postal code of the address associated with the territory. Maximum length is 20 characters.",
        "maxLength": 20,
        "nullable": true
      },
      "State": {
        "type": "string",
        "description": "The state of the address associated with the territory. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Street": {
        "type": "textarea",
        "description": "The street number and name of the address associated with the territory.",
        "maxLength": 255,
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Territory ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
