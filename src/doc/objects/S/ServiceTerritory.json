{
  "ServiceTerritory": {
    "name": "ServiceTerritory",
    "description": "Represents a geographic or functional region in which work can be performed in Field Service, Salesforce Scheduler, or Workforce Engagement.",
    "properties": {
      "Address": {
        "type": "address",
        "description": "An address to associate with the territory. For example, you can list the address of the territory’s headquarters.",
        "nullable": true,
        "readOnly": true
      },
      "City": {
        "type": "string",
        "description": "The city of the associated address. Maximum length is 40 characters.",
        "maxLength": 40,
        "nullable": true
      },
      "Country": {
        "type": "string",
        "description": "The country to associate with the territory. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the territory.",
        "maxLength": 32000,
        "nullable": true
      },
      "GeocodeAccuracy": {
        "type": "picklist",
        "description": "The level of accuracy of a location’s geographical coordinates compared with its physical address. Usually provided by a geocoding service based on the address’s latitude and longitude coordinates.This field is available in the API only.",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "IsActive": {
        "type": "boolean",
        "description": "Indicates whether the service territory is meant to be used. If a territory is inactive, you can’t add members to it or link it to work orders, work order line items, or service appointments.",
        "nullable": false
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the territory was last modified. Its label in the user interface is Last Modified Date.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the territory was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "Latitude": {
        "type": "double",
        "description": "Used with Longitude to specify the precise geolocation of the address associated with the territory. Acceptable values are numbers between –90 and 90 with up to 15 decimal places.This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Longitude": {
        "type": "double",
        "description": "Used with Latitude to specify the precise geolocation of the address associated with the territory. Acceptable values are numbers between –180 and 180 with up to 15 decimal places.This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Name": {
        "type": "string",
        "description": "The name of the territory.",
        "maxLength": 255,
        "nullable": false
      },
      "OperatingHoursId": {
        "type": "reference",
        "description": "The territory’s operating hours, which indicate when service appointments within the territory can occur. Service resources who are members of a territory automatically inherit the territory’s operating hours unless different hours are specified on the resource record.",
        "format": "salesforce-id",
        "x-object": "OperatingHours",
        "maxLength": 18,
        "nullable": false
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the record.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ParentTerritoryId": {
        "type": "reference",
        "description": "The territory’s parent service territory, if it has one. For example, a Northern California territory can have a State of California territory as its parent. A service territory hierarchy can contain up to 10,000 territories.",
        "format": "salesforce-id",
        "x-object": "ServiceTerritory",
        "maxLength": 18,
        "nullable": true
      },
      "PostalCode": {
        "type": "string",
        "description": "The postal code of the address associated with the territory. Maximum length is 20 characters.",
        "maxLength": 20,
        "nullable": true
      },
      "State": {
        "type": "string",
        "description": "The state of the address associated with the territory. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Street": {
        "type": "textarea",
        "description": "The street number and name of the address associated with the territory.",
        "maxLength": 255,
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Territory ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "description": "Last Modified By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "description": "System Modstamp",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "description": "May Edit",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "description": "Is Locked",
        "nullable": false,
        "readOnly": true
      },
      "TopLevelTerritoryId": {
        "type": "reference",
        "description": "(Read only) The top-level territory in a hierarchy of service territories. Depending on where a territory lies in the hierarchy, its top-level territory can be the same as its parent.",
        "format": "salesforce-id",
        "x-object": "ServiceTerritory",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "TypicalInTerritoryTravelTime": {
        "type": "double",
        "description": "Estimated number of minutes needed to travel from one location to another within the service territory. You can use this field in Apex customization.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "SchedulingConstraintId": {
        "type": "string",
        "description": "Constraint ID",
        "format": "salesforce-id",
        "x-object": "SchedulingConstraint",
        "maxLength": 18,
        "nullable": true
      },
      "FSL__Internal_SLR_Geolocation__Latitude__s": {
        "type": "number",
        "description": "Internal SLR HomeAddress Geolocation (Latitude)",
        "nullable": true
      },
      "FSL__Internal_SLR_Geolocation__Longitude__s": {
        "type": "number",
        "description": "Internal SLR HomeAddress Geolocation (Longitude)",
        "nullable": true
      },
      "FSLDemoTools_Original_Geolocation__Latitude__s": {
