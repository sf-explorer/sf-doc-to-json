{
  "SearchActivity": {
    "name": "SearchActivity",
    "description": "Represents search activity on a Knowledge article. Also known as KnowledgeSearchActivity.",
    "properties": {
      "AvgNumResults": {
        "type": "double",
        "description": "The number of search results returned for the search term. If Period is also included, this value is aggregated based on the time period specified.",
        "nullable": false,
        "readOnly": true,
        "multipleOf": 0.00009999999999999999
      },
      "ClickRank": {
        "type": "double",
        "description": "The order that the article appeared in the search results when the user sorted the results by relevance and clicked it from the list of results.",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.00009999999999999999
      },
      "ClickedRecordId": {
        "type": "reference",
        "description": "ID of the clicked article.",
        "format": "salesforce-id",
        "x-object": "Knowledge__kav",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ClickedRecordName": {
        "type": "string",
        "description": "The title of the clicked article taken when the user sorts the search results by relevance.",
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
      "CountQueries": {
        "type": "int",
        "description": "The number of searches for the period (day, month, or year).",
        "nullable": false,
        "readOnly": true
      },
      "CountUsers": {
        "type": "int",
        "description": "The number of individual users who clicked the article.",
        "nullable": false,
        "readOnly": true
      },
      "KbChannel": {
        "type": "picklist",
        "description": "The channel that’s applicable to the article.",
        "format": "enum",
        "enum": [
          "AllChannels",
          "App",
          "Pkb",
          "Csp",
          "Prm"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "Name": {
        "type": "string",
        "description": "Name of search activity.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "Period": {
        "type": "picklist",
        "description": "The time period that the search count is applied to. For example, a record where the Count is 70 and the Period is Monthly indicates that 70 searches took place over the past month. Totals are aggregated daily for the current month, monthly from the past full month through the past full year, and yearly beyond that.",
        "format": "enum",
        "enum": [
          "DAY",
          "MONTH",
          "YEAR"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "QueryDate": {
        "type": "date",
        "description": "The date of the search.",
        "format": "date",
        "nullable": false,
        "readOnly": true
      },
      "QueryLanguage": {
        "type": "picklist",
        "description": "The language filter that’s applied to the user’s search.",
        "format": "enum",
        "enum": [
          "en_US",
          "fr"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "SearchTerm": {
        "type": "string",
        "description": "The first 100 characters of the search term that was used to search published articles in the knowledge base.",
        "maxLength": 100,
        "nullable": false,
        "readOnly": true
      },
      "Id": {
        "type": "string",
        "description": "Search Activity Id",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "description": "Last Modified By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "description": "System Modstamp",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "description": "May Edit",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "description": "Is Locked",
        "nullable": false,
        "readOnly": true
      }
    },
    "module": "Core Salesforce",
    "sourceUrl": "https://developer.salesforce.com/docs/atlas.en-us.object_reference.meta/object_reference/sforce_api_objects_searchactivity.htm",
    "keyPrefix": "0JO",
    "createable": false,
    "updateable": false,
    "deletable": false,
    "queryable": true,
    "searchable": false,
    "childRelationships": [
      {
        "childObject": "FlowExecutionErrorEvent",
        "field": "ContextRecordId",
        "relationshipName": null,
        "cascadeDelete": false
      },
      {
        "childObject": "FlowRecordRelation",
        "field": "RelatedRecordId",
        "relationshipName": null,
        "cascadeDelete": false
      }
    ],
    "accessRules": "The Knowledge Base Search Dashboard permission must be enabled in your org.",
    "clouds": [
      "Core Salesforce"
    ]
  }
}