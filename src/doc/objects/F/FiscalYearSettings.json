{
  "FiscalYearSettings": {
    "name": "FiscalYearSettings",
    "description": "Settings to define a custom or standard fiscal year for your organization. This object has a parent-child relationship with the Period object.",
    "properties": {
      "Id": {
        "type": "string",
        "description": "Fiscal Year Settings ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "PeriodId": {
        "type": "reference",
        "format": "salesforce-id",
        "x-object": "Period",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true,
        "description": "ID of the associated fiscal period."
      },
      "StartDate": {
        "type": "date",
        "format": "date",
        "nullable": true,
        "readOnly": true,
        "description": "Start date of the fiscal year."
      },
      "EndDate": {
        "type": "date",
        "format": "date",
        "nullable": true,
        "readOnly": true,
        "description": "End date of the fiscal year."
      },
      "Name": {
        "type": "string",
        "maxLength": 80,
        "nullable": false,
        "readOnly": true,
        "description": "A name for the fiscal year. Limit: 80 characters."
      },
      "IsStandardYear": {
        "type": "boolean",
        "nullable": false,
        "readOnly": true,
        "description": "Indicates whether the fiscal year is a standard calendar year (true) or a custom fiscal year (false)."
      },
      "YearType": {
        "type": "picklist",
        "format": "enum",
        "enum": [
          "Standard",
          "Custom",
          "Placeholder"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true,
        "description": "Indicates one of two types of fiscal years, Standard or Custom. Standard denotes the standard Gregorian calendar, while Custom means a fiscal year with a custom structure."
      },
      "QuarterLabelScheme": {
        "type": "picklist",
        "description": "The numbering scheme used for fiscal quarters.",
        "format": "enum",
        "enum": [
          "NumberByYear",
          "Custom"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true
      },
      "PeriodLabelScheme": {
        "type": "picklist",
        "description": "The numbering scheme used for fiscal periods.",
        "format": "enum",
        "enum": [
          "NumberByYear",
          "NumberByQuarter",
          "StandardMonths",
          "Custom"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true
      },
      "WeekLabelScheme": {
        "type": "picklist",
        "description": "The numbering scheme used for weeks.",
        "format": "enum",
        "enum": [
          "NumberByYear",
          "NumberByQuarter",
          "NumberByPeriod"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true
      },
      "QuarterPrefix": {
        "type": "picklist",
        "format": "enum",
        "enum": [
          "Quarter",
          "FQ",
          "Q",
          "Trimester"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true,
        "description": "The prefix of fiscal quarters. For example, if “Q” is the prefix, then the fourth quarter would be “Q4.”"
      },
      "PeriodPrefix": {
        "type": "picklist",
        "format": "enum",
        "enum": [
          "Period",
          "FP",
          "P",
          "Month"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true,
        "description": "The prefix of fiscal periods. For example, if p is the prefix, then the first period is “P1.”"
      },
      "WeekStartDay": {
        "type": "int",
        "nullable": true,
        "readOnly": true,
        "description": "The name of the day that starts the week, for example Monday or Sunday"
      },
      "Description": {
        "type": "string",
        "maxLength": 255,
        "nullable": true,
        "readOnly": true,
        "description": "Description of the setting."
      },
      "SystemModstamp": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      }
    },
    "module": "Core Salesforce",
    "sourceUrl": "https://developer.salesforce.com/docs/atlas.en-us.object_reference.meta/object_reference/sforce_api_objects_fiscalyearsettings.htm",
    "keyPrefix": "022",
    "label": "Fiscal Year Settings",
    "createable": false,
    "updateable": false,
    "deletable": false,
    "queryable": true,
    "searchable": false,
    "childRelationships": [
      {
        "childObject": "Period",
        "field": "FiscalYearSettingsId",
        "relationshipName": "Periods",
        "cascadeDelete": false
      }
    ],
    "accessRules": "As of Spring ’20 and later, only partner users and standard users can access this object.",
    "clouds": [
      "Core Salesforce",
      "Core Salesforce"
    ]
  }
}