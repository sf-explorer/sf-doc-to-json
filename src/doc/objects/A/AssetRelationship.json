{
  "AssetRelationship": {
    "name": "AssetRelationship",
    "description": "Represents a non-hierarchical relationship between assets due to an asset modification; for example, a replacement, upgrade, or other circumstance. In Subscription Management and Revenue Lifecycle Management, this object represents an asset or assets grouped in a bundle or set.",
    "properties": {
      "AssetId": {
        "type": "reference",
        "description": "The unique identifier of the new asset, which is the asset that is taking the place of the existing asset.",
        "format": "salesforce-id",
        "x-object": "Asset",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "AssetRelationshipNumber": {
        "type": "string",
        "description": "An auto-generated number identifying the asset relationship.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "AssetRole": {
        "type": "picklist",
        "description": "Describes the position of the main asset relative to the other assets in the relationship."
      },
      "CurrencyIsoCode": {
        "type": "picklist",
        "description": "Three-letter ISO 4217 currency code associated with the asset. The default value is USD."
      },
      "FromDate": {
        "type": "dateTime",
        "description": "The date when the new asset was installed.",
        "format": "date-time",
        "nullable": true
      },
      "GroupingKey": {
        "type": "string",
        "description": "Read-only field used to indicate the bundle that an asset belongs to. For example, if two assets have the same GroupingKey value, then it means that the assets are bundled together."
      },
      "ProductRelationshipTypeId": {
        "type": "reference",
        "description": "The unique identifier of the record that describes the relationship between the main and associated assets."
      },
      "ProductRelatedComponent": {
        "type": "reference",
        "description": "The product related component that’s associated with the asset relationship."
      },
      "RelatedAssetId": {
        "type": "reference",
        "description": "The existing asset that is being modified.",
        "format": "salesforce-id",
        "x-object": "Asset",
        "maxLength": 18,
        "nullable": false
      },
      "RelatedAssetPricing": {
        "type": "picklist",
        "description": "Specifies whether the price of the related asset is included in the bundle price. Valid values are: IncludedInBundlePrice NotIncludedInBundlePrice"
      },
      "RelatedAssetQtyScaleMethod": {
        "type": "picklist",
        "description": "Specifies how the quantity of the related asset changes relative to the quantity of the parent asset. Valid values are: Constant Proportional"
      },
      "RelatedAssetRole": {
        "type": "picklist",
        "description": "Describes the position of the associated asset relative to other assets in the relationship."
      },
      "RelationshipType": {
        "type": "picklist",
        "description": "The type of relationship between the existing asset and the new asset. This field comes with three values—Replacement, Upgrade, and Crossgrade—, but you can create more values in Setup.",
        "format": "enum",
        "enum": [
          "Replacement",
          "Upgrade",
          "Crossgrade"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ToDate": {
        "type": "dateTime",
        "description": "The date when the modified asset is uninstalled.",
        "format": "date-time",
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Asset Relationship ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
