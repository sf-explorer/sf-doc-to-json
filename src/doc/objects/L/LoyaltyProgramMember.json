        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "nullable": false,
        "readOnly": true
      },
      "CurrencyIsoCode": {
        "type": "string",
        "description": "CurrencyIsoCode field",
        "format": "enum",
