{
  "LoyaltyProgramMember": {
    "name": "LoyaltyProgramMember",
    "description": "Represents information about member who belongs to a loyalty program. The member can be individual or corporate type.",
    "properties": {
      "AccountId": {
        "type": "reference",
        "description": "ID of the account with which the loyalty program member is associated. This field is optional for a Group type member.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "CanReceivePartnerPromotions": {
        "type": "boolean",
        "description": "Indicates whether the member can receive partner promotions for the program (true) or not (false).",
        "nullable": false
      },
      "CanReceivePromotions": {
        "type": "boolean",
        "description": "Indicates whether the member can receive promotions for the program (true) or not (false).",
        "nullable": false
      },
      "ContactId": {
        "type": "reference",
        "description": "ID of the contact with which the loyalty program member is associated. This field is optional for a Group type member.",
        "format": "salesforce-id",
        "x-object": "Contact",
        "maxLength": 18,
        "nullable": true
      },
      "EnrollmentChannel": {
        "type": "picklist",
        "description": "Channel through which membership is enrolled.",
        "format": "enum",
        "enum": [
          "POS",
          "Web",
          "Email",
          "CallCenter",
          "Social"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "EnrollmentDate": {
        "type": "date",
        "description": "Required. Date when the member enrolled to the loyalty program.",
        "format": "date",
        "nullable": false
      },
      "GroupCreatedByMemberId": {
        "type": "reference",
        "description": "The Individual or Corporate type loyalty program member who created the group or requested for the group to be created. This field is available in API version 53.0 and later."
      },
      "GroupName": {
        "type": "string",
        "description": "The name of the group when the type of membership in the loyalty program is Group. This field is available in API version 53.0 and later."
      },
      "IsEligibleForTierAssessment": {
        "type": "boolean",
        "description": "Indicates whether the member is eligible for tier assessment. A member is eligible for tier assessment when there is a change in their qualifying points balance. This field is available in API version 58.0 and later."
      },
      "LastActivityDate": {
        "type": "date",
        "description": "The date when the loyalty program member last performed a transaction.",
        "format": "date",
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The most recent date on which a user referenced this record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The most recent date on which a user viewed this record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "MemberStatus": {
        "type": "picklist",
        "description": "Required. The status of the member in the loyalty program.",
        "format": "enum",
        "enum": [
          "Active",
          "Inactive",
          "Merged"
        ],
        "maxLength": 40,
        "nullable": false
      },
      "MemberType": {
        "type": "picklist",
        "description": "Required. The type of membership in the loyalty program.",
        "format": "enum",
        "enum": [
          "Individual",
          "Corporate"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "MembershipEndDate": {
        "type": "date",
        "description": "The date on which the membership ends.",
        "format": "date",
        "nullable": true
      },
      "MembershipLastRenewalDate": {
        "type": "date",
        "description": "The most recent date when the membership was renewed.",
        "format": "date",
        "nullable": true
      },
      "MembershipNumber": {
        "type": "string",
        "description": "The membership number of the loyalty program.",
        "maxLength": 255,
        "nullable": false
      },
      "OwnerId": {
        "type": "reference",
        "description": "The ID of the user who owns this record.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ProgramId": {
        "type": "reference",
        "description": "ID of the loyalty program.",
        "format": "salesforce-id",
        "x-object": "LoyaltyProgram",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ReferralCode": {
        "type": "string",
        "description": "The code that the member can share with others who want to join the loyalty program. If no referral code is entered during enrollment, an automatic referral code is generated for the member. This field is available in API version 57.0 and later."
      },
      "ReferredById": {
        "type": "reference",
        "description": "Reference to Member who referred to the program.",
        "format": "salesforce-id",
        "x-object": "LoyaltyProgramMember",
        "maxLength": 18,
        "nullable": true
      },
      "RelatedCorporateMembershipId": {
        "type": "reference",
        "description": "The related corporate membership ID of the loyalty program.",
        "format": "salesforce-id",
        "x-object": "LoyaltyProgramMember",
        "maxLength": 18,
        "nullable": true
      },
      "StatementFrequency": {
        "type": "picklist",
        "description": "The frequency of loyalty balance statements.",
        "format": "enum",
        "enum": [
          "Monthly",
          "Quarterly"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "StatementLastGeneratedDate": {
        "type": "dateTime",
        "description": "The date on which the statement is last generated.",
        "format": "date-time",
        "nullable": true
      },
      "StatementMethod": {
        "type": "picklist",
        "description": "The mode of loyalty balance statements.",
        "format": "enum",
        "enum": [
          "Mail",
          "Email"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Loyalty Program Member ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
