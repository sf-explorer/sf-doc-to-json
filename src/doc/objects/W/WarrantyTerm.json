{
  "WarrantyTerm": {
    "name": "WarrantyTerm",
    "description": "Represents warranty terms defining the labor, parts, and expenses covered, along with any exchange options, provided to rectify issues with products.",
    "properties": {
      "Code": {
        "type": "string",
        "description": "A code or other identifier associated with this warranty term.",
        "maxLength": 255,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the warranty term.",
        "maxLength": 32000,
        "nullable": true
      },
      "EffectiveStartDate": {
        "type": "picklist",
        "description": "The date on which the warranty term became available for use.",
