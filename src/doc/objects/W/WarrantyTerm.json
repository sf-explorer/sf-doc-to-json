          "Install Date",
          "Manufacture Date",
          "Purchase Date"
        ],
        "maxLength": 40,
        "nullable": false
      },
      "ExchangeType": {
        "type": "picklist",
        "description": "The type of exchange offered.",
        "format": "enum",
