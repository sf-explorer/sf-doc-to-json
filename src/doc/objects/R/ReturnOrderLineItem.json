{
  "ReturnOrderLineItem": {
    "name": "ReturnOrderLineItem",
    "description": "Represents a specific product that is returned or repaired as part of a return order in Field service, or a specific order item that is returned as part of a return order in Order Management.",
    "properties": {
      "AssetId": {
        "type": "reference",
        "description": "The asset associated with the return order line item. One or more of the following fields must be filled out: AssetId, OrderItemId, Product2Id, ProductItemId, and ProductRequestLineItemId.",
        "format": "salesforce-id",
        "x-object": "Asset",
        "maxLength": 18,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "Notes or context about the return order line item.",
        "maxLength": 32000,
        "nullable": true
      },
      "DestinationLocationId": {
        "type": "reference",
        "description": "The location where the items are being returned to. For example, if the return order tracks the return of products from a technician’s van to a warehouse, the warehouse is the destination location.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the return order line item was last modified. Its label in the user interface is Last Modified Date.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the return order line item was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "OrderItemId": {
        "type": "reference",
        "description": "The order product associated with the return order line item. One or more of the following fields must be filled out: AssetId, OrderItemId, Product2Id, ProductItemId, and ProductRequestLineItemId.",
        "format": "salesforce-id",
        "x-object": "OrderItem",
        "maxLength": 18,
        "nullable": true
      },
      "ProcessingPlan": {
        "type": "picklist",
        "description": "Indicates the preferred fate of the items following their return. Available values are: Repair—Repair the items and return them to the owner Discard—Discard the items Salvage—Salvage the items’ working parts Restock—Return the items to your inventory",
        "format": "enum",
        "enum": [
          "Repair",
          "Discard",
          "Salvage",
          "Restock"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "Product2Id": {
        "type": "reference",
        "description": "The product associated with the return order line item. One or more of the following fields must be filled out: AssetId, OrderItemId, Product2Id, ProductItemId, and ProductRequestLineItemId.",
        "format": "salesforce-id",
        "x-object": "Product2",
        "maxLength": 18,
        "nullable": true
      },
      "ProductItemId": {
        "type": "reference",
        "description": "The product item representing the location of the product at the start of the return. One or more of the following fields must be filled out: AssetId, OrderItemId, Product2Id, ProductItemId, and ProductRequestLineItemId.",
        "format": "salesforce-id",
        "x-object": "ProductItem",
        "maxLength": 18,
        "nullable": true
      },
      "ProductRequestLineItemId": {
        "type": "reference",
        "description": "The product request line item associated with the return order line item. One or more of the following fields must be filled out: AssetId, OrderItemId, Product2Id, ProductItemId, and ProductRequestLineItemId.",
        "format": "salesforce-id",
        "x-object": "ProductRequestLineItem",
        "maxLength": 18,
        "nullable": true
      },
      "QuantityExpected": {
        "type": "double",
        "description": "The quantity of items expected to be returned.",
        "nullable": true
      },
      "QuantityReceived": {
        "type": "double",
        "description": "The actual quantity of items received for return.",
        "nullable": true
      },
      "QuantityRejected": {
        "type": "double",
        "description": "The quantity of items rejected for return.",
        "nullable": true
      },
      "QuantityReturned": {
        "type": "double",
        "description": "The quantity of items being returned. If multiple types of products are being returned, track each product in a different return order line item.",
        "nullable": false,
        "multipleOf": 0.01
      },
      "QuantityUnitOfMeasure": {
        "type": "picklist",
        "description": "Units of the returned items; for example, kilograms or liters. Quantity Unit of Measure picklist values are inherited from the Quantity Unit of Measure field on products.",
        "format": "enum",
        "enum": [
          "Each"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ReasonForRejection": {
        "type": "picklist",
        "description": "Reason for rejecting returned items on this return order line item.",
        "format": "enum",
        "enum": [
          "Damaged Item",
          "Missing Item or Part",
          "Wrong Item",
          "Expired Warranty"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ReasonForReturn": {
        "type": "picklist",
        "description": "The reason the items are being returned. Available values are: Damaged Defective Duplicate Order Wrong Item Wrong Quantity Not Satisfied Outdated Other",
        "format": "enum",
        "enum": [
          "Damaged",
          "Defective",
          "Duplicate Order",
          "Wrong Item",
          "Wrong Quantity",
          "Not Satisfied",
          "Outdated",
          "Other"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ReasonForChangeText": {
        "type": "string",
        "description": "Details about the reason for return change"
      },
      "RepaymentMethod": {
        "type": "picklist",
        "description": "The method by which the customer or owner will be reimbursed for the items being returned. Available values are: Replace—The items will be replaced Refund—The items will be returned and the owner will be refunded Credit—The items will be returned and the owner will receive credit for them Return—The items will be returned to the owner (for example, following their repair)",
        "format": "enum",
        "enum": [
          "Replace",
          "Refund",
          "Credit",
          "Return"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ReturnOrderId": {
        "type": "reference",
        "description": "The return order that the return order line item belongs to.",
        "format": "salesforce-id",
        "x-object": "ReturnOrder",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "ReturnOrderLineItemNumber": {
        "type": "string",
        "description": "(Read only) Auto-generated number that identifies the return order line item.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "SourceLocationId": {
        "type": "reference",
        "description": "The items’ location at the start of the return or repair. For example, if the return order tracks the return of products from a technician’s service vehicle to a warehouse, the service vehicle is the source location.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Return Order Line Item ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "description": "Last Modified By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "description": "System Modstamp",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "description": "May Edit",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "description": "Is Locked",
        "nullable": false,
        "readOnly": true
      },
      "OrderItemSummaryId": {
        "type": "reference",
        "description": "ID of the order item summary associated with the return order line item.",
        "format": "salesforce-id",
        "x-object": "OrderItemSummary",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ChangeOrderItemId": {
        "type": "reference",
        "description": "ID of the change order item associated with the return order line item.",
        "format": "salesforce-id",
        "x-object": "OrderItem",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "Type": {
        "type": "picklist",
        "description": "Type of the return order line item. Matches the type of the associated order item summary. Delivery Charge indicates that the return order line item represents a delivery charge. Fee indicates that it represents another type of fee, such as a return fee. Order Product indicates that it represents any other type of product, service, or charge. Each type corresponds to one type code, shown here in parentheses.",
        "format": "enum",
        "enum": [
          "Order Product",
          "Delivery Charge",
          "Fee"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "TypeCode": {
        "type": "picklist",
        "description": "Type code of the return order line item. Matches the type code of the associated order item summary. Processing depends on this value. Charge indicates that the return order line item represents a delivery charge. Product indicates that it represents an other type of product, service, or charge. Each type category corresponds to one or more types.",
        "format": "enum",
        "enum": [
          "Product",
          "Charge"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "UnitPrice": {
        "type": "currency",
        "description": "Unit price of the return order line item.",
        "format": "currency",
        "nullable": true,
        "multipleOf": 0.01
      },
      "GrossUnitPrice": {
        "type": "currency",
        "description": "Unit price, including tax, of the product represented by the associated order item summary.",
        "format": "currency",
        "nullable": true,
        "multipleOf": 0.01
      },
      "TotalLineAmount": {
        "type": "currency",
        "description": "Total, not including adjustments or tax, of the return order line item.",
        "format": "currency",
        "nullable": true,
        "multipleOf": 0.01
      },
      "TotalLineTaxAmount": {
        "type": "currency",
        "description": "Tax on the TotalLineAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAdjustmentTaxAmount": {
        "type": "currency",
        "description": "Tax on the TotalAdjustmentAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAdjustmentAmount": {
        "type": "currency",
        "description": "Total of all price adjustments applied to the return order line item.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalLineAmountWithTax": {
        "type": "currency",
        "description": "Total price of the return order line item, inclusive of tax. This amount is equal to TotalLineAmount + TotalLineTaxAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAdjustmentAmountWithTax": {
        "type": "currency",
        "description": "Total amount of the price adjustments applied to the return order line item, inclusive of tax. This amount is equal to TotalAdjustmentAmount + TotalAdjustmentTaxAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalPrice": {
        "type": "currency",
        "description": "Total, including adjustments but not tax, of the return order line item. Equal to UnitPrice times Quantity.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalTaxAmount": {
        "type": "currency",
        "description": "Tax on the TotalAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAmount": {
        "type": "currency",
        "description": "Total, including adjustments and tax, of the return order line item.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "CurrencyIsoCode": {
        "type": "picklist",
        "description": "Available only for orgs with the multicurrency feature enabled. Contains the ISO code for the currency of the original Order associated with the ReturnOrderLineItem.",
        "format": "enum",
        "enum": [
          "USD"
        ],
        "maxLength": 3,
        "nullable": false,
        "readOnly": true
      },
      "ProductServiceCampaignId": {
        "type": "reference",
        "description": "The product service campaign associated with the return order line item."
      },
      "ProductServiceCampaignItemId": {
        "type": "reference",
        "description": "The product service campaign item associated with the return order line item."
      }
    },
