{
  "ReturnOrder": {
    "name": "ReturnOrder",
    "description": "Represents the return or repair of inventory or products in Field Service, or the return of order products in Order Management.",
    "properties": {
      "AccountId": {
        "type": "reference",
        "description": "The account associated with the return order.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "CaseId": {
        "type": "reference",
        "description": "The case associated with the return order.",
        "format": "salesforce-id",
        "x-object": "Case",
        "maxLength": 18,
        "nullable": true
      },
      "ContactId": {
        "type": "reference",
        "description": "The contact associated with the return order.",
        "format": "salesforce-id",
        "x-object": "Contact",
        "maxLength": 18,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "Notes or context about the return order.",
        "maxLength": 32000,
        "nullable": true
      },
      "DestinationLocationId": {
        "type": "reference",
        "description": "The location where the items are being returned to. For example, if the return order tracks the return of products from a technician’s van to a warehouse, the warehouse is the destination location.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": true
      },
      "ExpectedArrivalDate": {
        "type": "dateTime",
        "description": "The date when the items are expected to arrive at the destination location.",
        "format": "date-time",
        "nullable": true
      },
      "ExpirationDate": {
        "type": "dateTime",
        "description": "Authorizations can’t be captured after their expiration dates.",
        "format": "date-time",
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the return order was last modified. Its label in the user interface is Last Modified Date.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the return order was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "OrderId": {
        "type": "reference",
        "description": "The order associated with the return order. When you associated a return order with an order, you can associate the return order’s line items with order products.",
        "format": "salesforce-id",
        "x-object": "Order",
        "maxLength": 18,
        "nullable": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the return order.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "ProductRequestId": {
        "type": "reference",
        "description": "The product request associated with the return order. When you associated a return order with a product request, you can associate the return order’s line items with the product request’s line items.A return order might be related to a product request if the return order tracks the return of unused products or products to be repaired or replaced. For example, a technician creates a product request for three motors to prepare for a field visit. If the technician finds that only two motors are needed, they can create a return order to return the third to the original location, and list the product request in this field.",
        "format": "salesforce-id",
        "x-object": "ProductRequest",
        "maxLength": 18,
        "nullable": true
      },
      "ReturnOrderNumber": {
        "type": "string",
        "description": "(Read only) Auto-generated number identifying the return order.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ReturnedById": {
        "type": "reference",
        "description": "ID of the user returning the items.",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": true
      },
      "ShipFromAddress": {
        "type": "address",
        "description": "The return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "nullable": true,
        "readOnly": true
      },
      "ShipFromCity": {
        "type": "string",
        "description": "The city of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "maxLength": 40,
        "nullable": true
      },
      "ShipFromCountry": {
        "type": "string",
        "description": "The country of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "maxLength": 80,
        "nullable": true
      },
      "ShipFromGeocodeAccuracy": {
        "type": "picklist",
        "description": "Accuracy level of the geocode for the return shipping address. See Compound Field Considerations and Limitations for details on geolocation compound fields. Note This field is available in the API only.",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "ShipFromLatitude": {
        "type": "double",
        "description": "Used with Longitude to specify the precise geolocation of the return shipping address. Acceptable values are numbers between –90 and 90 with up to 15 decimal places. See Compound Field Considerations and Limitations for details on geolocation compound fields. Note This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "ShipFromLongitude": {
        "type": "double",
        "description": "Used with Latitude to specify the precise geolocation of the return shipping address. Acceptable values are numbers between –180 and 180 with up to 15 decimal places. See Compound Field Considerations and Limitations for details on geolocation compound fields. Note This field is available in the API only.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "ShipFromPostalCode": {
        "type": "string",
        "description": "The postal code of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "maxLength": 20,
        "nullable": true
      },
      "ShipFromState": {
        "type": "string",
        "description": "The state of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "maxLength": 80,
        "nullable": true
      },
      "ShipFromStreet": {
        "type": "textarea",
        "description": "The street of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "maxLength": 255,
        "nullable": true
      },
      "ShipmentType": {
        "type": "picklist",
        "description": "The type of shipment associated with the return order. Available values are: Standard (default value) Rush Overnight Next Business Day Pick Up",
        "format": "enum",
        "enum": [
          "Standard",
          "Rush",
          "Overnight",
          "Next Business Day",
          "Pick Up"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "SourceLocationId": {
        "type": "reference",
        "description": "The items’ location at the start of the return or repair. For example, if the return order tracks the return of products from a technician’s service vehicle to a warehouse, the service vehicle is the source location.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": true
      },
      "Status": {
        "type": "picklist",
        "description": "The status of the return order. Available values are: Draft Submitted Approved Canceled Closed",
        "format": "enum",
        "enum": [
          "Draft",
          "Submitted",
          "Approved",
          "Canceled",
          "Closed",
          "Received",
          "Completed",
          "Pending Exchange"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "StatusCategory": {
        "type": "picklist",
        "description": "Status category of the return order. Processing of the return order depends on this value. Each status category corresponds to one or more statuses.",
        "format": "enum",
        "enum": [
          "Draft",
          "Activated",
          "Closed",
          "Canceled",
          "Pending"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
      "Id": {
        "type": "string",
        "description": "Return Order ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
