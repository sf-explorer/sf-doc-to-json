{
  "WorkType": {
    "name": "WorkType",
    "description": "Represents a type of work to be performed in Field Service and Lightning Scheduler. Work types are templates that can be applied to work order or work order line items.",
    "properties": {
      "Id": {
        "type": "string",
        "description": "Work Type ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "The work type’s owner.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "Name": {
        "type": "string",
        "description": "The name of the work type. Try to use a name that helps users quickly understand the type of work orders that can be created from the work type. For example, “Annual Refrigerator Maintenance” or “Valve Replacement.”",
        "maxLength": 255,
        "nullable": false
      },
      "CreatedDate": {
        "type": "string",
        "description": "Created Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "CreatedById": {
        "type": "string",
        "description": "Created By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedDate": {
        "type": "string",
        "description": "Last Modified Date",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "LastModifiedById": {
        "type": "string",
        "description": "Last Modified By ID",
        "format": "salesforce-id",
        "x-object": "User",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "SystemModstamp": {
        "type": "string",
        "description": "System Modstamp",
        "format": "date-time",
        "nullable": false,
        "readOnly": true
      },
      "MayEdit": {
        "type": "boolean",
        "description": "May Edit",
        "nullable": false,
        "readOnly": true
      },
      "IsLocked": {
        "type": "boolean",
        "description": "Is Locked",
        "nullable": false,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the work type was last viewed by the current user.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the work type was last modified. Its label in the user interface is Last Modified Date.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the work type. Try to add details about the task or tasks that this work type represents.",
        "maxLength": 32000,
        "nullable": true
      },
      "EstimatedDuration": {
        "type": "double",
        "description": "The estimated length of the work. The estimated duration is in minutes or hours based on the value selected in the Duration Type field.",
        "nullable": false,
        "multipleOf": 0.01
      },
      "DurationType": {
        "type": "picklist",
        "description": "The unit of the Estimated Duration: Minutes or Hours.",
        "format": "enum",
        "enum": [
          "Hours",
          "Minutes"
        ],
        "maxLength": 255,
        "nullable": false
      },
      "DurationInMinutes": {
        "type": "number",
        "description": "Duration in Minutes",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "ShouldAutoCreateSvcAppt": {
        "type": "boolean",
        "description": "Select this option to have a service appointment automatically created on work orders and work order line items that use the work type. Note By default, the Due Date on auto-created service appointments is seven days after the created date. Admins can adjust this offset from the Field Service Settings page in Setup. If a work type with the Auto-Create Service Appointment option selected is added to an existing work order or work order line item, a service appointment is only created for the work order or work order line item if it doesn’t yet have one. If someone updates an existing work type by selecting the Auto-Create Service Appointment option, service appointments aren’t created on work orders and work order line items that were already using the work type.",
        "nullable": false
      },
      "ServiceReportTemplateId": {
        "type": "reference",
        "description": "The service report template associated with the work type. When users create service reports from a work order or work order line item that uses this work type, the reports use this template.",
        "format": "salesforce-id",
        "x-object": "ServiceReportLayout",
        "maxLength": 18,
        "nullable": true
      },
      "MinimumCrewSize": {
        "type": "int",
        "description": "The minimum crew size allowed for a crew assigned to the work. Work orders and work order line items inherit their work type’s minimum crew size.If you’re not using the Field Service managed package, this field serves as a suggestion rather than a rule. If you are using the managed package, the scheduling optimizer counts the number of service crew members on a service crew to determine whether it fits the minimum crew size requirement.",
        "nullable": true
      },
      "RecommendedCrewSize": {
        "type": "int",
        "description": "The recommended number of people on the service crew assigned to the work. For example, you might have a Minimum Crew Size of 2 and a Recommended Crew Size of 3. Work orders and work order line items inherit their work type’s recommended crew size.",
        "nullable": true
      },
      "TimeframeStart": {
        "type": "integer",
        "description": "Timeframe Start",
        "nullable": true
      },
      "TimeframeEnd": {
        "type": "integer",
        "description": "Timeframe End",
        "nullable": true
      },
      "BlockTimeBeforeAppointment": {
        "type": "integer",
        "description": "Block Time Before Appointment",
        "nullable": true
      },
      "BlockTimeAfterAppointment": {
        "type": "integer",
        "description": "Block Time After Appointment",
        "nullable": true
      },
      "DefaultAppointmentType": {
        "type": "string",
        "description": "Default Appointment Type",
        "format": "enum",
        "enum": [
          "video",
          "In a Center"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "TimeFrameStartUnit": {
        "type": "string",
        "description": "Time Frame Start Unit",
        "format": "enum",
        "enum": [
          "Days",
          "Hours"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "TimeFrameEndUnit": {
        "type": "string",
        "description": "Time Frame End Unit",
        "format": "enum",
        "enum": [
          "Days",
          "Hours"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "BlockTimeBeforeUnit": {
        "type": "string",
        "description": "Block Time Before Unit",
        "format": "enum",
        "enum": [
          "Minutes",
          "Hours"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "BlockTimeAfterUnit": {
        "type": "string",
        "description": "Block Time After Unit",
        "format": "enum",
        "enum": [
          "Minutes",
          "Hours"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "OperatingHoursId": {
        "type": "string",
        "description": "Operating Hour ID",
        "format": "salesforce-id",
        "x-object": "OperatingHours",
        "maxLength": 18,
        "nullable": true
      },
      "SaDocumentTemplate": {
        "type": "string",
        "description": "The document template ID. If ServiceDocumentTemplateId isn’t specified, this document template ID determines which service document template is used for service documents generated from a service appointment. The ID is 15 to 18 characters long."
      },
      "WoDocumentTemplate": {
        "type": "string",
        "description": "The document template ID. If ServiceDocumentTemplateId isn’t specified, this document template ID determines which service document template is used for service documents generated from a work order. The ID is 15 to 18 characters long."
      },
      "WoliDocumentTemplate": {
        "type": "string",
        "description": "The document template ID. If ServiceDocumentTemplateId isn’t specified, this document template ID determines which service document template is used for service documents generated from a work order line item. The ID is 15 to 18 characters long."
      },
      "CurrencyIsoCode": {
        "type": "string",
        "description": "CurrencyIsoCode field",
        "format": "enum",
        "enum": [
          "USD"
        ],
        "maxLength": 3,
        "nullable": true
      }
    },
