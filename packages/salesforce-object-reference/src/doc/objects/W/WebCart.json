{
  "WebCart": {
    "name": "WebCart",
    "description": "Represents an online shopping cart for a store built with B2B Commerce or D2C Commerce, with total amounts for products, shipping and handling, and taxes.",
    "properties": {
      "AccountId": {
        "type": "reference",
        "description": "ID of the account that owns this WebCart. In API version 51.0 and later, if the WebCart was created through Guest Browsing, this ID is the ID of the GuestBuyerProfile.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "BillingAddress": {
        "type": "address",
        "description": "The mailing address to which this WebCart is billed.",
        "nullable": true,
        "readOnly": true
      },
      "BillingCity": {
        "type": "string",
        "description": "The city of the billing address.",
        "maxLength": 40,
        "nullable": true
      },
      "BillingCountry": {
        "type": "string",
        "description": "The country of the billing address.",
        "maxLength": 80,
        "nullable": true
      },
      "BillingGeocodeAccuracy": {
        "type": "picklist",
        "description": "The accuracy rating of the geocode for the billing address. Possible values are: Address Block City County ExtendedZip NearAddress Neighborhood State Street Unknown Zip",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "BillingLatitude": {
        "type": "double",
        "description": "The latitude of the geocode for the billing address.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "BillingLongitude": {
        "type": "double",
        "description": "The longitude of the geocode for the billing address.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "BillingPostalCode": {
        "type": "string",
        "description": "The postal code for the billing address.",
        "maxLength": 20,
        "nullable": true
      },
      "BillingState": {
        "type": "string",
        "description": "The state of the billing address.",
        "maxLength": 80,
        "nullable": true
      },
      "BillingStreet": {
        "type": "textarea",
        "description": "The street of the billing address. Enter up to 255 characters.",
        "maxLength": 255,
        "nullable": true
      },
      "CurrencyIsoCode": {
        "type": "picklist",
        "description": "The ISO code for the currency that’s specified on the buyer’s account. Default value is USD. Possible values are:",
        "format": "enum",
        "enum": [
          "USD"
        ],
        "maxLength": 3,
        "nullable": true
      },
      "GrandTotalAmount": {
        "type": "currency",
        "description": "Sum of all cart items’ TotalAmount, or WebCart TotalAmount plus WebCart TotalTaxAmount. This value includes all taxes and adjustments.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "GuestCompanyName": {
        "type": "string",
        "description": "Company name associated with a delivery for a guest customer. This field is available in API version 59.0 and later."
      },
      "GuestEmailAddress": {
        "type": "email",
        "description": "The email address of a guest buyer."
      },
      "GuestFirstName": {
        "type": "string",
        "description": "The first name of a guest buyer."
      },
      "GuestLastName": {
        "type": "string",
        "description": "The last name (or surname) of a guest buyer."
      },
      "GuestPhoneNumber": {
        "type": "phone",
        "description": "The phone number of a guest buyer."
      },
      "GuestSecondName": {
        "type": "string",
        "description": "The second name of a guest buyer."
      },
      "InitialOrderReferenceNumber": {
        "type": "string",
        "description": "A unique identifier assigned to the WebCart at the beginning of the checkout process. Initially populated when the checkout process is initiated, the InitialOrderReferenceNumber is then associated with the order created when the checkout is complete."
      },
      "InventoryReservationIdentifier": {
        "type": "string",
        "description": "Reserved for future use."
      },
      "IsRepricingNeeded": {
        "type": "boolean",
        "description": "Whether the cart has changed since the last repricing. The default value is false.",
        "nullable": false
      },
      "IsSecondary": {
        "type": "boolean",
        "description": "Whether the cart is a secondary cart or a primary cart."
      },
      "LastRepricingDate": {
        "type": "dateTime",
        "description": "Date when the last repricing was done.",
        "format": "date-time",
        "nullable": true
      },
      "Name": {
        "type": "string",
        "description": "The name of this WebCart record. Name can be up to 255 characters.",
        "maxLength": 255,
        "nullable": false
      },
      "OwnerId": {
        "type": "reference",
        "description": "The ID of the owner of this WebCart.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "PaymentGroupId": {
        "type": "reference",
        "description": "The ID of the WebCart payment group.",
        "format": "salesforce-id",
        "x-object": "PaymentGroup",
        "maxLength": 18,
        "nullable": true
      },
      "PaymentMethodId": {
        "type": "reference",
        "description": "The method of payment for this WebCart.",
        "format": "salesforce-id",
        "x-objects": [
          "CardPaymentMethod",
          "DigitalWallet"
        ],
        "maxLength": 18,
        "nullable": true
      },
      "PoNumber": {
        "type": "string",
        "description": "The purchase order number. Enter up to 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Status": {
        "type": "picklist",
        "description": "The status of this WebCart. Possible values are: Active—Cart is created and available for modifications, like adding or removing products or promotions. Checkout—Cart is in checkout. If the customer modifies the cart, the current checkout session is canceled. Closed—Checkout is complete and an order was created. The cart cannot be modified. PendingClosed—Cart is marked to be closed, but the request isn't completed yet. The cart can’t be modified. This value is available in API version 57.0 and later. PendingDelete—Cart is marked for delete, but the request isn't completed yet. The cart can’t be modified. Processing—Cart is processing. For example, taxes are being calculated. The cart can’t be modified.",
        "format": "enum",
        "enum": [
          "Active",
          "Closed",
          "PendingClosed",
          "Processing",
          "Checkout",
          "PendingDelete"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "TaxLocaleType": {
        "type": "picklist",
        "description": "The type of tax locale. Possible values are: Net Gross"
      },
      "TaxType": {
        "type": "picklist",
        "description": "The type of tax policy. Possible values are: Automatic Net Gross"
      },
      "TotalAdjustmentAmount": {
        "type": "currency",
        "description": "A calculated field that reflects the total of all adjustments to the cart subtotal. Adjustments include item, tier, and cart level discounts."
      },
      "TotalAmount": {
        "type": "currency",
        "description": "The sum of all cart items’ TotalPrice, or TotalProductAmount plus TotalChargeAmount. If the store tax type is Gross, the sum includes taxes.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalAmountAfterAllAdjustments": {
        "type": "currency",
        "description": "The sum of all cart items after all price adjustments are applied. Adjustments include various types of discounts."
      },
      "TotalCartLevelAdjAmount": {
        "type": "currency",
        "description": "Total cart level discount amount for the cart."
      },
      "TotalChargeAmount": {
        "type": "currency",
        "description": "The sum of all cart items’ TotalPrice for cart items of the type Charge.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalChargeTaxAmount": {
        "type": "currency",
        "description": "The sum of all the cart items’ TotalTaxAmount for cart items of the type Charge.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalLineItemsWithErrors": {
        "type": "int",
        "description": "A calculated field that shows the total number of cart line items of type Product with errors.",
        "nullable": true,
        "readOnly": true
      },
      "TotalListAmount": {
        "type": "currency",
        "description": "Sum of all the cart items’ TotalListPrice.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalProductAmount": {
        "type": "currency",
        "description": "The sum of all the cart items’ TotalPrice for cart items of the type Product.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalProductCount": {
        "type": "double",
        "description": "A count of all the products in the WebCart.",
        "nullable": true,
        "readOnly": true
      },
      "TotalProductItemAdjAmount": {
        "type": "currency",
        "description": "Total item level discount amount for the cart."
      },
      "TotalProductLineItemCount": {
        "type": "int",
        "description": "A calculated field that shows the total number of cart line items of type Product."
      },
      "TotalProductListAmount": {
        "type": "currency",
        "description": "The sum of all the cart items’ TotalListAmount for the CartItem type Product."
      },
      "TotalProductTaxAmount": {
        "type": "currency",
        "description": "The sum of all the cart items’ TotalTaxAmount for the CartItem type Product.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "TotalPromoAdjustmentAmount": {
        "type": "currency",
        "description": "The total of all item discounts related to product promotions."
      },
      "TotalPromoAdjustmentTaxAmount": {
        "type": "currency",
        "description": "The total tax adjustment for all item discounts related to product promotions."
      },
      "TotalTaxAmount": {
        "type": "currency",
        "description": "The sum of all cart items’ TotalTaxAmount, or TotalProductTaxAmount plus TotalDeliveryTaxAmount.",
        "format": "currency",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.01
      },
      "Type": {
        "type": "picklist",
        "description": "The WebCart type. Default value is Cart. Possible values are: Cart PayNowReadOnly ReadOnly Template",
        "format": "enum",
        "enum": [
          "Cart",
          "Template",
          "ReadOnly",
          "PayNowReadOnly"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "UniqueProductCount": {
        "type": "int",
        "description": "The count of unique product SKUs in the WebCart.",
        "nullable": true,
        "readOnly": true
      },
