        "maxLength": 200,
        "nullable": true
      },
      "MaxResponseTimeOperator": {
        "type": "int",
        "description": "The maximum time in seconds it took an agent to respond to a chat visitor’s message.",
        "nullable": true
      },
      "MaxResponseTimeVisitor": {
        "type": "int",
        "description": "The maximum time in seconds it took a customer to respond to an agent’s message.",
        "nullable": true
      },
      "Name": {
        "type": "string",
        "description": "The name of the transcript.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
