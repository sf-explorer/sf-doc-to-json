{
  "LiveChatTranscript": {
    "name": "LiveChatTranscript",
    "description": "This object is automatically created for each Live Agent chat session and stores information about the session.",
    "properties": {
      "Abandoned": {
        "type": "int",
        "description": "The amount of time in seconds an incoming chat request remained unanswered by an agent before the chat was disconnected by the customer.",
        "nullable": true,
        "readOnly": true
      },
      "AccountId": {
        "type": "ID",
        "description": "ID of the account associated with the chat transcript.",
        "format": "salesforce-id",
        "x-object": "Account",
        "maxLength": 18,
        "nullable": true
      },
      "AverageResponseTimeOperator": {
        "type": "int",
        "description": "The agent’s average response time (in seconds) to chat messages from the visitor.",
        "nullable": true
      },
      "AverageResponseTimeVisitor": {
        "type": "int",
        "description": "The visitor’s average response time (in seconds) to chat messages from the agent.",
        "nullable": true
      },
      "Body": {
        "type": "textarea",
        "description": "The contents of the chat.",
        "maxLength": 131072,
        "nullable": true
      },
      "Browser": {
        "type": "string",
        "description": "The browser the visitor used for the chat.",
        "maxLength": 200,
        "nullable": true
      },
      "BrowserLanguage": {
        "type": "string",
        "description": "The language of the visitor’s browser.",
        "maxLength": 200,
        "nullable": true
      },
      "CaseID": {
        "type": "reference",
        "description": "The ID of the case associated with the chat transcript."
      },
      "ChatDuration": {
        "type": "int",
        "description": "The total duration of the chat in seconds.",
        "nullable": true,
        "readOnly": true
      },
      "ChatKey": {
        "type": "string",
        "description": "The session ID of the chat before it is persisted. ChatKey can be used with advanced integrations in the Salesforce console. This field is available in API version 25.0 and later.",
        "maxLength": 200,
        "nullable": true,
        "readOnly": true
      },
      "ContactID": {
        "type": "reference",
        "description": "The ID of the contact associated with the chat transcript."
      },
      "EndedBy": {
        "type": "picklist",
        "description": "The way the chat was ended: by the operator, the visitor, or the system.",
        "format": "enum",
        "enum": [
          "Agent",
          "Visitor",
          "System"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "EndTime": {
        "type": "dateTime",
        "description": "The time the chat ended.",
        "format": "date-time",
        "nullable": true
      },
      "IpAddress": {
        "type": "string",
        "description": "The auto-populated visitor’s IP address. Do not edit. Create a custom field if you need an IP address field for your use case.",
        "maxLength": 39,
        "nullable": true
      },
      "IsChatbotSession": {
        "type": "boolean",
        "description": "Whether the visitor is chatting with a chatbot (true) or not (false).",
        "nullable": false,
        "readOnly": true
      },
      "LastReferencedDate": {
        "type": "date",
        "description": "The timestamp for when the current user last viewed a record related to this record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "date",
        "description": "The timestamp for when the current user last viewed this record. If this value is null, this record might only have been referenced (LastReferencedDate) and not viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LeadID": {
        "type": "reference",
        "description": "The ID of the lead associated with the chat transcript."
      },
      "LiveChatButtonID": {
        "type": "reference",
        "description": "The ID of the LiveChatButton the chat session originated from."
      },
      "LiveChatDeploymentID": {
        "type": "reference",
        "description": "The ID of the LiveChatDeployment the chat session originated from."
      },
      "LiveChatVisitorID": {
        "type": "reference",
        "description": "The ID of the visitor associated with the chat transcript."
      },
      "Location": {
        "type": "string",
