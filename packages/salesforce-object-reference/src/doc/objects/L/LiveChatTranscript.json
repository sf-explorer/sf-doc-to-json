        "x-object": "Skill",
        "maxLength": 18,
        "nullable": true
      },
      "StartTime": {
        "type": "dateTime",
        "description": "The time the chat started.",
        "format": "date-time",
        "nullable": true
      },
      "Status": {
        "type": "picklist",
