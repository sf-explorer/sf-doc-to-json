        "format": "enum",
        "enum": [
          "Fee Simple",
          "Leasehold"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "HasOtherLoanFeature": {
        "type": "boolean",
        "description": "Indicates whether loan has other amortization feature.",
        "nullable": false
      },
      "HasPrepaymentPenalty": {
        "type": "boolean",
        "description": "Indicates whether penalty is incurred if borrower pays off loan balance before the loan term expires.",
        "nullable": false
      },
      "ImprovementCost": {
        "type": "currency",
        "description": "Construction costs for a construction loan or home improvement costs for a renovation loan.",
        "format": "currency",
        "nullable": true,
        "multipleOf": 0.01
      },
      "InitialBuydownRate": {
        "type": "double",
        "description": "Initial lesser interest rate for the loan.",
        "nullable": true,
        "multipleOf": 0.001
      },
      "IntOnlyTermMonthCount": {
        "type": "double",
        "description": "Number of months that loan payments are interest only.",
        "nullable": true
      },
      "InterestRate": {
        "type": "double",
        "description": "Interest rate for the loan.",
        "nullable": true,
        "multipleOf": 0.001
      },
      "IsBalloon": {
        "type": "boolean",
        "description": "Indicates whether borrower is applying for a balloon payment mortgage.",
        "nullable": false
      },
      "IsBorrowerCommPropState": {
        "type": "boolean",
        "description": "Indicates whether at least one borrower lives in a community property state.",
        "nullable": false
      },
      "IsConstructionConversion": {
        "type": "boolean",
        "description": "Indicates whether loan proceeds to provide permanent financing replacing interim construction financing.",
        "nullable": false
      },
      "IsContractConversion": {
        "type": "boolean",
        "description": "Indicates whether loan proceeds is to pay off balance due on a contract for deed or land contract.",
        "nullable": false
      },
      "IsEnergyRelImprovement": {
        "type": "boolean",
        "description": "Indicates whether loan is to finance energy-related improvements.",
        "nullable": false
      },
      "IsInterestOnly": {
        "type": "boolean",
        "description": "Indicates whether borrower is applying for an interest-only mortgage.",
        "nullable": false
      },
      "IsNegativeAmortization": {
        "type": "boolean",
        "description": "Indicates whether loan allows for negative amortization where shortfalls in interest payments are added to the outstanding loan balance.",
        "nullable": false
      },
      "IsPropertyCommPropState": {
        "type": "boolean",
        "description": "Indicates whether property is located in a community property state.",
        "nullable": false
      },
      "IsRenovation": {
        "type": "boolean",
        "description": "Indicates whether loan proceeds to finance cost of renovations to the property, and renovation value is included in the property's appraised value.",
        "nullable": false
      },
      "IsSingleClosing": {
        "type": "boolean",
        "description": "Indicates whether construction and permanent loans have a single combined closing. Once construction is complete, the loan converts to a traditional mortgage.",
        "nullable": false
      },
      "IsSubjectToPriorityLien": {
        "type": "boolean",
        "description": "Indicates whether property is currently subject to an energy-related lien that could take priority over the first mortgage lien.",
        "nullable": false
      },
      "IsTempIntRateBuydown": {
        "type": "boolean",
        "description": "Indicates whether borrower is applying for loan with an initially reduced interest payment in exchange for an up-front cash deposit.",
        "nullable": false
      },
