        "type": "picklist",
        "description": "Context in which the macro was run. Possible values are: Aloha—Salesforce Classic Lightning—Lightning Experience Unknown",
        "format": "enum",
        "enum": [
          "Aloha",
          "Lightning",
          "Unknown"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
