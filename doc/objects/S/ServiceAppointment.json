        "type": "reference",
        "description": "The parent record related to the appointment. The parent record can’t be updated after the service appointment is created. For test drive appointments, the parent record is a lead or an opportunity. For vehicle service appointments, theparent record is an asset that’s related to the vehicle.",
        "format": "salesforce-id",
        "x-objects": [
          "Account",
          "Asset",
          "CareProgramEnrollee",
          "Case",
          "Lead",
          "Opportunity",
          "WorkOrder",
          "WorkOrderLineItem"
        ],
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ParentRecordType": {
        "type": "string",
        "description": "(Read only) The type of parent record that provides context for the appointment, such as Asset, Lead, or Opportunity.",
        "maxLength": 50,
        "nullable": true,
        "readOnly": true
      },
      "PostalCode": {
        "type": "string",
        "description": "The postal code where the appointment is completed. Maximum length is 20 characters.",
        "maxLength": 20,
        "nullable": true
      },
      "SchedEndTime": {
