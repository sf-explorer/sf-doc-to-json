{
  "ServiceAppointment": {
    "name": "ServiceAppointment",
    "description": "Represents a service appointment booked through Automotive Scheduler, for test drives or vehicle services.",
    "properties": {
      "ActualDuration": {
        "type": "double",
        "description": "The number of minutes that it took the resource to complete the appointment. When values are first added to the Actual Start and Actual End fields, the Actual Duration is automatically populated to list the difference between the Actual Start and Actual End. If the Actual Start and Actual End fields are subsequently updated, the Actual Duration field doesn’t re-update, but you can manually update it.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "ActualEndTime": {
        "type": "dateTime",
        "description": "The actual date and time the appointment ended.",
        "format": "date-time",
        "nullable": true
      },
      "ActualStartTime": {
        "type": "dateTime",
        "description": "The actual date and time the appointment started.",
        "format": "date-time",
        "nullable": true
      },
      "AdditionalInformation": {
        "type": "string",
        "description": "Represents additional information about the service appointment.",
        "maxLength": 255,
        "nullable": true
      },
      "Address": {
        "type": "address",
        "description": "The address where the appointment is taking place.",
        "nullable": false,
        "readOnly": true
      },
      "AppointmentNumber": {
        "type": "string",
        "description": "An auto-assigned number that identifies the appointment.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ArrivalWindowEndTime": {
        "type": "dateTime",
        "description": "The end of the window of time in which the technician is scheduled to arrive at the site.",
        "format": "date-time",
        "nullable": true
      },
      "ArrivalWindowStartTime": {
        "type": "dateTime",
        "description": "The beginning of the window of time in which the technician is scheduled to arrive at the site.",
        "format": "date-time",
        "nullable": true
      },
      "CancellationReason": {
        "type": "string",
        "description": "The reason for the service appointment cancellation.",
        "maxLength": 255,
        "nullable": true
      },
      "City": {
        "type": "string",
        "description": "The city where the appointment is completed. Maximum length is 40 characters.",
        "maxLength": 40,
        "nullable": true
      },
      "Comments": {
        "type": "string",
        "description": "The comments for the service appointment.",
        "maxLength": 255,
        "nullable": true
      },
      "Country": {
        "type": "string",
        "description": "The country where the appointment is completed. Maximum length is 80 characters.",
        "maxLength": 80,
        "nullable": true
      },
      "Description": {
        "type": "textarea",
        "description": "The description of the appointment.",
        "maxLength": 32000,
        "nullable": true
      },
      "DueDate": {
        "type": "dateTime",
        "description": "The date by which the appointment must be completed.",
        "format": "date-time",
        "nullable": false
      },
      "Duration": {
        "type": "double",
        "description": "The estimated length of the appointment as derived from the associated work type. The duration is in minutes or hours based on the value selected in the Duration Type field.",
        "nullable": true,
        "multipleOf": 0.01
      },
      "DurationType": {
        "type": "picklist",
        "description": "The unit of duration as specified for the associated work type.",
        "format": "enum",
        "enum": [
          "Hours",
          "Minutes"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "EarliestStartTime": {
        "type": "dateTime",
        "description": "The date after which the appointment must be completed.",
        "format": "date-time",
        "nullable": false
      },
      "GeocodeAccuracy": {
        "type": "picklist",
        "description": "The level of accuracy of a location’s geographical coordinates compared with its physical address. Usually provided by a geocoding service based on the address’s latitude and longitude coordinates.",
        "format": "enum",
        "enum": [
          "Address",
          "NearAddress",
          "Block",
          "Street",
          "ExtendedZip",
          "Zip",
          "Neighborhood",
          "City",
          "County",
          "State",
          "Unknown"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date when the service appointment was last modified.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date when the service appointment was last viewed.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "Latitude": {
        "type": "double",
        "description": "Used with Longitude to specify the precise geolocation of the address where the service appointments is completed. Acceptable values are numbers between –90 and 90 with up to 15 decimal places.",
        "nullable": true,
        "multipleOf": 1e-15
      },
      "Longitude": {
        "type": "double",
        "description": "Used with Latitude to specify the precise geolocation of the address where the service appointment is completed. Acceptable values are numbers between –180 and 180 with up to 15 decimal places.",
        "nullable": true,
        "multipleOf": 1e-15
      },
