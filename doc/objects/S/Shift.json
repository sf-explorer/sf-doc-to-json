{
  "Shift": {
    "name": "Shift",
    "description": "Represents a shift for service resource scheduling.",
    "properties": {
      "BackgroundColor": {
        "type": "string",
        "description": "Sets a background color when shifts are displayed in the UI. Use a 3- or 6-digit hexadecimal format, for example #FF00FF. Available in API version 54.0 and later."
      },
      "EndLocation": {
        "type": "location",
        "description": "Captures the user’s geolocation at the end of the shift."
      },
      "EndTime": {
        "type": "dateTime",
        "description": "The date and time that the shift ends.",
        "format": "date-time",
        "nullable": false
      },
      "GeoFnLcTypeAtShftEnd": {
        "type": "picklist",
        "description": "Specifies the allowed location type for geofence validation at the end of the shift."
      },
      "GeoFnLcTypeAtShftStart": {
        "type": "picklist",
        "description": "Specifies the allowed location type for geofence validation at the start of the shift."
      },
      "GeoFnNotificationType": {
        "type": "picklist",
        "description": "Specifies the notification type for geofence validation at the start and end of the shift."
      },
      "IsGeoFnRqrAtShftEnd": {
        "type": "boolean",
        "description": "Indicates whether geofencing was enabled at the end of the shift."
      },
      "IsGeoFnRqrAtShftStart": {
        "type": "boolean",
        "description": "Indicates whether geofencing was enabled at the start of the shift."
      },
      "IsGeoLcRqrAtShftEnd": {
        "type": "boolean",
        "description": "Indicates whether user's geolocation was captured at the end of the shift."
      },
      "IsGeoLcRqrAtShftStart": {
        "type": "boolean",
        "description": "Indicates whether user's geolocation was captured at the start of the shift."
      },
      "IsHolidayShift": {
        "type": "boolean",
        "description": "Indicates a shift that overlaps with holiday hours. The default value is false. Available in API version 55.0 and later."
      },
      "IsNonStandard": {
        "type": "boolean",
        "description": "Indicates whether the shift is nonstandard, such as overtime or on-call shifts."
      },
      "IsOsidGeoFnRngAtShftEnd": {
        "type": "boolean",
        "description": "Indicates whether the shift was ended outside the allowed distance between the user's GPS location and the geofence end location (true) or not (false)."
      },
      "IsOsidGeoFnRngAtShftStart": {
        "type": "boolean",
        "description": "Indicates whether the shift was started outside the allowed distance between the user's GPS location and the geofence start location (true) or not (false)."
      },
      "JobProfileId": {
        "type": "reference",
        "description": "The job profile associated with the shift. Available in API versions 47.0 and later.",
        "format": "salesforce-id",
        "x-object": "JobProfile",
        "maxLength": 18,
        "nullable": true
      },
      "Label": {
        "type": "string",
        "description": "The label that a shift is given.",
        "maxLength": 255,
        "nullable": true
      },
      "LastReferencedDate": {
        "type": "dateTime",
        "description": "The date and time when the current user last viewed a related record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LastViewedDate": {
        "type": "dateTime",
        "description": "The date and time when the current user last viewed this record.",
        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "NotificationTypeAtShftEnd": {
        "type": "picklist",
        "description": "Specifies the notification type for open tours at the end of the shift."
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the shift.",
        "format": "salesforce-id",
        "x-objects": [
          "Group",
          "User"
        ],
        "maxLength": 18,
        "nullable": false
      },
      "RecordsetFilterCriteriaId": {
        "type": "reference",
        "description": "The ID of the recordset filter criteria selected for the shift. Available in API version 49.0 and later.",
        "format": "salesforce-id",
        "x-object": "RecordsetFilterCriteria",
        "maxLength": 18,
        "nullable": true
      },
      "ServiceResourceId": {
        "type": "reference",
        "description": "The ID of the service resource the shift belongs to. Available in API versions 47.0 and later.",
        "format": "salesforce-id",
        "x-object": "ServiceResource",
        "maxLength": 18,
        "nullable": true
      },
      "ServiceTerritoryId": {
        "type": "reference",
        "description": "The ID of the service territory the shift belongs to. Available in API versions 47.0 and later.",
        "format": "salesforce-id",
        "x-object": "ServiceTerritory",
        "maxLength": 18,
        "nullable": true
      },
      "ShiftNumber": {
        "type": "string",
        "description": "The number automatically given to the shift upon creation.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ShiftTemplateId": {
        "type": "reference",
        "description": "The shift template ID, if the shift was created from a shift template. Available in API version 53.0 and later."
      },
      "ShouldAutoEnd": {
        "type": "boolean",
        "description": "Indicates whether the shift was automatically ended when the tour was completed."
      },
      "ShouldAutoStart": {
        "type": "boolean",
        "description": "Indicates whether the shift was automatically started when the tour began."
      },
      "StartLocation": {
        "type": "location",
        "description": "Captures the user’s geolocation at the start of the shift."
      },
      "StartTime": {
        "type": "dateTime",
        "description": "The date and time that the shift starts.",
        "format": "date-time",
        "nullable": false
      },
      "Status": {
        "type": "picklist",
        "description": "The current status of the shift.",
        "format": "enum",
        "enum": [
          "Tentative",
          "Published",
          "Confirmed"
        ],
        "maxLength": 40,
        "nullable": false
      },
      "StatusCategory": {
        "type": "picklist",
        "description": "Specifies the status of the shift using static values. This field is derived from Status using the mapping defined in setup.",
        "format": "enum",
        "enum": [
          "Tentative",
          "Published",
          "Confirmed"
        ],
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
      "TimeSlotType": {
        "type": "picklist",
        "description": "Type of time slot for the shift. The same setup values as the TimeSlot field in the OperatingHours object.",
        "format": "enum",
        "enum": [
          "Normal",
          "Extended"
        ],
        "maxLength": 255,
        "nullable": false
      },
      "UserId": {
        "type": "reference",
        "description": "The user assigned to the shift."
      },
      "Id": {
        "type": "string",
        "description": "Shift ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
        "description": "Deleted",
        "nullable": false,
        "readOnly": true
      },
      "CreatedDate": {
        "type": "string",
