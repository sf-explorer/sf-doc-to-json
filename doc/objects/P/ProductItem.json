        "format": "date-time",
        "nullable": true,
        "readOnly": true
      },
      "LocationId": {
        "type": "reference",
        "description": "The location that’s associated with the product item. This usually indicates where the product item is stored.",
        "format": "salesforce-id",
        "x-object": "Location",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "OwnerId": {
        "type": "reference",
        "description": "The owner of the product item.",
        "format": "salesforce-id",
