        "x-object": "Product2",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "ProductItemNumber": {
        "type": "string",
        "description": "The auto-generated number identifying the product item.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "ProductName": {
        "type": "string",
        "description": "The name of the product item. We recommend you use a name that indicates the name of the product along with its location.",
        "maxLength": 255,
        "nullable": true,
        "readOnly": true
      },
      "QuantityOnHand": {
        "type": "double",
        "description": "The quantity at the location. If you want to add a serial number, this value must be 1.",
        "nullable": false,
        "multipleOf": 0.01
      },
      "QuantityUnitOfMeasure": {
        "type": "picklist",
        "description": "The unit of the quantity of the product that’s consumed. For example, kilograms or liters. Quantity Unit of Measure values are inherited from the Quantity Unit of Measure field on products.",
