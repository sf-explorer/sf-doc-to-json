        "enum": [
          "Each"
        ],
        "maxLength": 40,
        "nullable": true
      },
      "SerialNumber": {
        "type": "string",
        "description": "A unique number for identification purposes. If you want to enter a serial number, the Quantity on Hand must be 1.",
        "maxLength": 80,
        "nullable": true
      },
      "Id": {
        "type": "string",
        "description": "Product Item ID",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
