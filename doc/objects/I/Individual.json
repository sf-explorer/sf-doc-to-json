        "nullable": false,
        "readOnly": true
      },
      "Occupation": {
        "type": "string",
        "description": "The customer’s occupation. Maximum size is 150 characters.",
        "maxLength": 150,
        "nullable": true
      },
      "OwnerId": {
        "type": "reference",
