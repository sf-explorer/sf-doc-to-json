        "x-object": "Individual",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "MilitaryService": {
        "type": "picklist",
        "description": "Indicates whether the customer has served in the military.",
        "format": "enum",
        "enum": [
          "Unknown",
          "Yes",
          "No"
        ],
        "maxLength": 255,
        "nullable": true
      },
      "Name": {
        "type": "string",
