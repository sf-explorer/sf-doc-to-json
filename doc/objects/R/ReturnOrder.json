        "x-object": "User",
        "maxLength": 18,
        "nullable": true
      },
      "ShipFromAddress": {
        "type": "address",
        "description": "The return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
        "nullable": true,
        "readOnly": true
      },
      "ShipFromCity": {
        "type": "string",
        "description": "The city of the return shipping address. This address tracks the location of the items at the start of the return or repair. For example, if a customer is returning an item, the Ship From address is the customer’s address.",
