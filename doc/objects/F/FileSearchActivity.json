          "no",
          "hu",
          "pl",
          "cs",
          "tr",
          "in",
          "ro",
          "vi",
          "uk",
          "iw",
          "el",
          "bg",
          "en_GB",
          "ar",
          "sk",
          "pt_PT",
          "hr",
          "sl"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "SearchTerm": {
        "type": "string",
        "description": "The first 100 characters of the search term that was used to search published files.",
        "maxLength": 100,
        "nullable": false,
        "readOnly": true
      },
      "Id": {
        "type": "string",
        "description": "Search Activity Id",
        "format": "salesforce-id",
        "maxLength": 18,
        "nullable": false,
        "readOnly": true
      },
      "IsDeleted": {
        "type": "boolean",
