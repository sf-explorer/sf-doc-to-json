{
  "FileSearchActivity": {
    "name": "FileSearchActivity",
    "description": "Represents search activity on a file.",
    "properties": {
      "AvgNumResults": {
        "type": "double",
        "description": "The number of search results returned for the search term. If Period is also included, this value is aggregated based on the time period specified.",
        "nullable": false,
        "readOnly": true,
        "multipleOf": 0.1
      },
      "ClickRank": {
        "type": "double",
        "description": "The order that the file appeared in the search results when users clicked it from the list of results.",
        "nullable": true,
        "readOnly": true,
        "multipleOf": 0.1
      },
      "CountQueries": {
        "type": "int",
        "description": "The number of searches for the period (day, month, or year).",
        "nullable": false,
        "readOnly": true
      },
      "CountUsers": {
        "type": "int",
        "description": "The number of individual users who clicked the file.",
        "nullable": false,
        "readOnly": true
      },
      "Name": {
        "type": "string",
        "description": "Name of search activity.",
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "Period": {
        "type": "picklist",
        "description": "The time period that the search count is applied to. For example, a record where the Count is 70 and the Period is Monthly indicates that 70 searches took place over the past month. Totals are aggregated daily for the current month, monthly from the past full month through the past full year, and yearly beyond that.",
        "format": "enum",
        "enum": [
          "DAY",
          "MONTH",
          "YEAR"
        ],
        "maxLength": 255,
        "nullable": false,
        "readOnly": true
      },
      "QueryDate": {
        "type": "date",
        "description": "The date of the search.",
        "format": "date",
        "nullable": false,
        "readOnly": true
      },
      "QueryLanguage": {
        "type": "picklist",
        "description": "The language filter that’s applied to the user’s search.",
        "format": "enum",
        "enum": [
          "en_US",
          "de",
          "es",
          "fr",
          "it",
          "ja",
          "sv",
          "ko",
          "zh_TW",
