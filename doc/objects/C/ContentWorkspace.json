        "x-object": "ContentFolder",
        "maxLength": 18,
        "nullable": true,
        "readOnly": true
      },
      "ShouldAddCreatorMembership": {
        "type": "boolean",
        "description": "Automatically create a library membership for the user creating the library. Note this field isn’t meant for query and always returns false in query. This field is available in API version 40.0 and later.",
        "nullable": false,
        "readOnly": true
      },
      "TagModel": {
        "type": "picklist",
        "description": "The type of tagging assigned to a library. Valid values are: U — Unrestricted. No restrictions on tagging. Users can enter any tag when publishing or editing content. G — Guided. Users can enter any tag when publishing or editing content, but they’re also offered a list of suggested tags. R — Restricted. Users must choose from a list of suggested tags.",
        "format": "enum",
        "enum": [
          "U",
          "G",
          "R"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true
      },
      "WorkspaceImageId": {
        "type": "reference",
        "description": "ID of a library image. Image files can be assigned to libraries for branding and easy identification. Library image is visible to all users, even if they aren’t library members. This field is available in API version 43.0 and later.",
        "format": "salesforce-id",
        "x-object": "ContentAsset",
        "maxLength": 18,
        "nullable": true
      },
      "WorkspaceType": {
        "type": "picklist",
        "description": "Differentiates between different types of libraries. Valid values are: R — Regular library B — Org asset library This field is available in API version 39.0 and later.",
        "format": "enum",
        "enum": [
          "R",
          "B"
        ],
        "maxLength": 40,
        "nullable": true,
        "readOnly": true
      },
