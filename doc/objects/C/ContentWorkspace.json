{
  "ContentWorkspace": {
    "name": "ContentWorkspace",
    "description": "Represents a content library.",
    "properties": {
      "DefaultRecordTypeId": {
        "type": "reference",
        "description": "ID of the default content type for the library. Content types are the containers for custom fields in Salesforce CRM Content.",
